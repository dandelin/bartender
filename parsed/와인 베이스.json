[{"descs": {"\uc751\uc6a9": "\ub0c9\uac01\ub41c \uce75\ud14c\uc77c \uae00\ub77c\uc2a4\uc5d0 \uac78\ub7ec \ub530\ub978\ub2e4.", "\uc7ac\ub8cc": "\uc170\ub9ac [60 ml], \uc2a4\uc704\ud2b8\ubc84\ubb34\uc2a4 [30 ml], \uc624\ub80c\uc9c0\ube44\ud130\uc988 [2 Dash]", "\ub9cc\ub4e4\uae30": "\ubbf9\uc2f1\uae00\ub77c\uc2a4\uc5d0 \uc7ac\ub8cc\ub97c \ub123\uace0 \uc5bc\uc74c\uacfc \ud568\uaed8 \uc798 \uc11e\ub294\ub2e4.", "\uba54\ubaa8": "\uc2a4\uc704\ud2b8 \ubc84\ubb34\uc2a4 \ub300\uc2e0 \ub4dc\ub77c\uc774 \ubc84\ubb34\uc2a4\ub97c \uc4f0\uba74 \ubc40\ubd80(Bamboo)\uac00 \ub41c\ub2e4.", "\uae30\ubc95": "\uc9c1\uc811\ub123\uae30(Build)."}, "img_src": "\uc640\uc778 \ubca0\uc774\uc2a4_files/blank.gif", "name": "\uc544\ub3c4\ub2c8\uc2a4(Adonis)"}, {"descs": {"\uc7ac\ub8cc": "\ubc31\ud3ec\ub3c4\uc8fc [120 ml], \ud06c\ub9bc \ub4dc \uce74\uc2dc\uc2a4 [22 ml]", "\ub9cc\ub4e4\uae30": "\uc640\uc778\uae00\ub77c\uc2a4\uc5d0 \uc7ac\ub8cc\ub97c \ub530\ub974\uace0 \uac00\ubccd\uac8c \uc11e\ub294\ub2e4.", "\uc720\ub798": "\uc640\uc778\uc758 \uace0\ud5a5\uc5d0\uc11c \ud0dc\uc5b4\ub09c \uc2dd\uc804\uc8fc, \uc624\ub298\ub0a0 \uc138\uacc4\uc5d0\uac8c \ud06c\uac8c \uc720\ud589 \ud0a4\ub974\ub294 \ud504\ub791\n\t\uc2a4 \ubd80\ub974\uace0\ub274 \uc9c0\ubc29\uc758 \ub514\uc8f5 \uc2dc\uc7a5\uc744 \uc9c0\ub0b8 \uce90\ub17c \ud3a0\ub9ad\uc2a4 \ud0a4\ub974\uac00 \uace0\uc548\ud55c \uce75\ud14c\uc77c\n\t\ubd80\ub974\uace0\ub274\uc0b0\uc758 \ubc31\ud3ec\ub3c4\uc8fc\uc640 \ub514\uc8f5 \ud2b9\uc0b0\uc758 \ud06c\ub80c \ub4dc\uce74\uc2dc\uc2a4\ub97c \uc0ac\uc6a9\ud558\uace0 \uc788\ub2e4.\n\t\uc0dd\uaca8\ub09c \uc774\ud6c4 \ub514\uc8f5 \uc2dc\uc758 \uacf5\uc2dd \ub9ac\uc149\uc158\uc5d0\ub294 \uc2dd\uc804\uc8fc\ub85c\uc11c \ubc18\ub4dc\uc2dc \ub098\uc654\ub2e4\uace0 \ud558\uba70\n\t\uc624\ub298\ub0a0\uc5d0\ub294 \uc138\uacc4 \uacf3\uacf3\uc5d0\uc11c \uc560\uc74c\ub418\uace0 \uc788\ub2e4.", "\uba54\ubaa8": "\uc77c\ubc18\uc801\uc73c\ub85c \uc640\uc778\uc740 \ub2e4\ub978 \uc74c\ub8cc\uc640 \ud63c\ud569\ud558\uc9c0 \uc54a\ub294 \uac83\uc774 \uc88b\uc73c\ub098 \ud0a4\ub974\ub294 \uc640\uc778\n\t\ubca0\uc774\uc2a4 \uc911\uc5d0\uc11c \uac00\uc7a5 \uc798 \uc54c\ub824\uc9c4 \uce75\ud14c\uc77c\uc774\ub2e4. \uc774 \uce75\ud14c\uc77c\uc740 \uc81c 2\ucc28 \uc138\uacc4\ub300\uc804\n\t\ud6c4 \ud504\ub791\uc2a4\uc758 \ube0c\ub974\uace0\uc288 \uc9c0\ubc29\uc758 \uc2dc\uc7a5\uc744 \uc5ed\uc784\ud55c'\uceac\ub18d\ud3a0\ub9ac\uc2a4 \ud0a4\ub974' \ub77c\ub294 \uc0ac\ub78c\n\t\uc758 \uc774\ub984\uc5d0\uc11c \uc720\ub798\ub418\uc5c8\ub2e4.", "\uae30\ubc95": "\uc9c1\uc811\ub123\uae30(Build)."}, "img_src": "\uc640\uc778 \ubca0\uc774\uc2a4_files/adonis.jpg", "name": "\ud0a4\ub974(Kir)"}, {"descs": {"\uae30\ubc95": "\uc9c1\uc811\ub123\uae30(Build).", "\uc7ac\ub8cc": "\uc0f4\ud398\uc778 [75 ml],  \uc624\ub80c\uc9c0 \uc8fc\uc2a4 [75 ml]", "\uc7a5\uc2dd": "\uc624\ub80c\uc9c0 \uc870\uac01.", "\ub9cc\ub4e4\uae30": "\ud50c\ub86f \uae00\ub77c\uc2a4\uc5d0 \uc7ac\ub8cc\ub97c \ubd93\uace0 \uac00\ubccd\uac8c \uc800\uc5b4\uc900\ub2e4.", "\uba54\ubaa8": "\ubbf8\ubaa8\uc0ac\uaf43\uc758 \ub178\ub780\uc0c9\uc744 \uc0c1\uc9d5\ud558\uba70 \ud1a1 \uc3d8\ub294 \uc0f4\ud398\uc778\uc758 \uccad\ub7c9\uac10\uacfc \uc624\ub80c\uc9c0\uc8fc\uc2a4\uc758\n\t\ub9db\uc740 \ubd84\uc704\uae30\ub97c \ud55c\uce35 \ub354 \uc0b4\ub824 \uc8fc\ub294 \uc74c\ub8cc\uc774\ub2e4.", "\uc720\ub798": "\uccad\uc870\ud558\uace0 \uac00\ub828\ud55c \ubbf8\ubaa8\uc0ac\uc640 \uac19\uc740 \uc2f1\uc2f1\ud558\uba74\uc11c\ub3c4 \ubd80\ub4dc\ub7ec\uc6b4 \uce75\ud14c\uc77c \ubbf8\ubaa8\uc0ac\ub294\n\t\uadf8\ub798\uc2a4\uc5d0 \ub530\ub790\uc744 \ub54c\uc758 \uc0c9\uae54\uc774 \ubbf8\ubaa8\uc0ac \uaf43\uacfc \ube44\uc2b7\ud558\ub2e4\uace0 \ud558\uc5ec \ubbf8\ubaa8\uc0ac\ub780 \uc560\n\t\uce6d\uc73c\ub85c \ubd88\ub9ac\uac8c \ub41c \uce75\ud14c\uc77c \ud504\ub791\uc2a4\uc5d0\uc11c\ub294 \uc0bc\ud398\uc778 \uc544 \ub85c\ub791\uc96c\ub77c \ud558\uc5ec \uc218 \ubc31\ub144\n\t\uc804\ubd80\ud130 \uc0c1\ub958\uacc4\uae09\uc5d0\uc11c \uc560\uc6a9\ub418\uc5c8\ub2e4\uace0 \ud55c\ub2e4. \uc624\ub80c\uc9c0 \uc96c\uc2a4\uc758 \ud5a5\uae30\uc5d0 \ud488\uc704\uc788\ub294\n\t\uc0bc\ud398\uc778\uc774 \uacc1\ub4e4\uc5ec\uc838 \uc6b0\uc544\ud55c \uc0ac\uad50\uc7a5\uc5d0\uc11c \uac01\uad11\uc744 \ubc1b\uc558\ub2e4."}, "img_src": "\uc640\uc778 \ubca0\uc774\uc2a4_files/blank.gif", "name": "\ubbf8\ubaa8\uc0ac(Mimosa)"}, {"descs": {"\uae30\ubc95": "\uc9c1\uc811\ub123\uae30(Build).", "\uc7ac\ub8cc": "\ubc31\ud3ec\ub3c4\uc8fc [60 ml],  \uc18c\ub2e4\uc218", "\uc7a5\uc2dd": "\ub77c\uc784 \uc870\uac01.", "\ub9cc\ub4e4\uae30": "\uc5bc\uc74c\uc744 \ub123\uc740 \uace0\ube14\ub81b \uae00\ub77c\uc2a4\uc5d0 \uc7ac\ub8cc\ub97c \ub123\uace0 \uac00\ubccd\uac8c \uc800\uc5b4\uc900\ub2e4.", "\uba54\ubaa8": "\uc2a4\ud504\ub9ac\ucc98\uc758 \uc5b4\uc6d0\uc740 \ub3c5\uc77c\uc5b4\uc758 \ub41c\ub2e4\ub77c\ub294 \ub73b\uc758 [\uc2a4\ud504\ub9ac\uccb8(Spritzen)]\uc774 \uc5c8\n\t\uc73c\ub098 \ud604\uc7ac\ub294 \ubbf8\uad6d\uc2dd \ubc1c\uc74c\uc73c\ub85c \ubc14\ub00c\uc5b4 \ub300\uc911\ud654\ub418\uc5c8\ub2e4.", "\uc720\ub798": "\uc9dc\ub9bf\ud558\uace0 \uacbd\ucf8c\ud55c \ub9db, \uac74\uac15\uc9c0\ud5a5 \uc2dc\ub300\uc5d0 \uc778\uae30\ub97c \ud68d\ub4dd\ud55c \uc2a4\ud504\ub9ac\ucc98\ub294 \uc624\uc2a4\ud2b8\n\t\ub9ac\uc544\uc758 \uc798\uc2a4\ube0c\ub8e8\ud06c\uc5d0\uc11c \ud0dc\uc5b4\ub09c \uccad\ub7c9\uac10\uc774 \ub118\uce58\ub294 \uc9dc\ub9bf\ud55c \ub9db\uc758 \uce75\ud14c\uc77c \uc2a4\ud504\n\t\ub9ac\uccb8(Spritzen, \ud130\uc9c0\ub2e4)\uc5d0\uc11c \ub098\uc628\ub9d0\uc774\ub2e4."}, "img_src": "\uc640\uc778 \ubca0\uc774\uc2a4_files/kir.jpg", "name": "\uc2a4\ud504\ub9ac\ucc98(Spritzer)"}, {"descs": {"\uc7a5\uc2dd": "\uc624\ub80c\uc9c0, \uc2a4\ud2b8\ub85c.", "\uc7ac\ub8cc": "\uc640\uc778 [90 ml], \uc624\ub80c\uc9c0 \ud034\ub77c\uc18c [15 ml], \uadf8\ub808\ub098\ub518 \uc2dc\ub7fd [15 ml], \uc624\ub80c\uc9c0 \uc8fc\uc2a4 [30 ml], \n \uac00\ub8e8\uc5bc\uc74c", "\ub9cc\ub4e4\uae30": "\uace0\ube14\ub81b \uae00\ub77c\uc2a4\uc5d0 \uc7ac\ub8cc\ub97c \ub123\uace0 \ubc14\uc2a4\ud47c\uc73c\ub85c \uac00\ubccd\uac8c \uc813\ub294\ub2e4.", "\uc0c1\uc2dd": "\ub098\uc774\uc560\uac00\ub77c \ud3ed\ud3ec", "\uae30\ubc95": "\uc9c1\uc811\ub123\uae30"}, "img_src": "\uc640\uc778 \ubca0\uc774\uc2a4_files/blank.gif", "name": "\uc640\uc778\ucfe8\ub7ec(Wine Cooler)"}]