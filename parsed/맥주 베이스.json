[{"descs": {"\uc720\ub798": "\ubcf4\ub77c\ub294 \uace0\uadc0\ud55c \ube5b\uae54\uc758 \ub300\ud45c\uc801 \uc874\uc7ac. \uc11c\uad6c\uc5d0\uc11c\ub294 '\ubcf4\ub77c\ub85c \ud0dc\uc5b4\ub09c\ub2e4'\ub294 \uac83\n\t\uc740 \uba85\ubb38 \ucd9c\uc2e0. \ubcf4\ub77c\ub85c \uc2b9\uc9c4\ud55c\ub2e4'\ub294 \uac83\uc740 \uc655\uc704\uacc4\uc2b9\uc744 \uc758\ubbf8\ud55c\ub2e4. \uace0\ub300 \ub85c\ub9c8\n        \uc2dc\ub300\uc758 \ub85c\uc5f4 \ud37c\ud50c(\uc9c4\ud64d\uc0c9)\uc5d0\uc11c \uc720\ub798\ud55c\ub2e4.", "\uc7ac\ub8cc": "\ubc31\ud3ec\ub3c4\uc8fc [60 ml],\ub9e5\uc8fc", "\ub9cc\ub4e4\uae30": "\uc5bc\uc74c\uc774 3-4\uac1c \ub4e0 \uc794\uc5d0 \ubc31\ud3ec\ub3c4\uc8fc\ub97c \ub530\ub974\uace0 \ucc28\uac8c \ud55c \ub9e5\uc8fc\ub97c \ucc44\uc6cc \uac00\ubccd\uac8c \n          \uc813\ub294\ub2e4.", "\uc794": "\ud140\ube14\ub7ec \uae00\ub77c\uc2a4", "\uae30\ubc95": "\uc250\uc774\ud06c[Shake]"}, "img_src": "\ub9e5\uc8fc \ubca0\uc774\uc2a4_files/blank.gif", "name": "\ube44\uc5b4\uc2a4\ud504\ub9ac\ucc98(Beer Spritzer)"}, {"descs": {"\uc720\ub798": "\ub9e5\uc8fc\uc640 \uc0f4\ud398\uc778\uc774 \uc790\uc544\ub0b4\ub294 \ubca8\ubcb3\uacfc \uac19\uc740 \ubd80\ub4dc\ub7ec\uc6b4 \uac10\ucd09\uc5d0 \ube14\ub799 \ubca8\ubcb3\uc740 \ucd09\uac10\uc774\n        \ubd80\ub4dc\ub7fd\uace0 \uad11\ud0dd\uc774 \uc788\ub294 \uc637\uac10. \uac80\uc740 \ubca8\ubcb3\uc774\ub77c\ub294 \uc6b0\uc544\ud55c \uc774\ub984\uc744 \uac00\uc9c4 \uc774 \uce75\ud14c\uc77c\n\t\uc740 \uc544\uc77c\ub79c\ub4dc\uc758 \ub4dc\ub77c\uc774\ud55c \uc2a4\ud0c0\uc6b0\ud2b8 \ub9e5\uc8fc\uc640 \ud504\ub791\uc2a4\uc758 \uadc0\ubd80\uc778\uac19\uc774 \ud488\uc704\uc788\ub294 \uc0f4\ud398\n\t\uc778\uc774 \ud568\uaed8 \uc5b4\uc6b8\ub824 \ub9cc\ub4e4\uc5b4\uc9c4 \uac83\uc774\ub2e4. \uadf8 \ub9db\uc740 \ubca8\ubcb3\ucc98\ub7fc \ucd09\uac10\uc774 \uc88b\uace0 \ubd80\ub4dc\ub7fd\ub2e4.", "\uc7ac\ub8cc": "\ud751\ub9e5\uc8fc,\uc0f4\ud398\uc778", "\ub9cc\ub4e4\uae30": "\uae00\ub77c\uc2a4\uc5d0 \ub0c9\uac01\uc2dc\ud0a8 \ud751\ub9e5\uc8fc\uc640 \uc0f4\ud398\uc778\uc744 \uc88c\uc6b0\uc5d0\uc11c \ub3d9\uc2dc\uc5d0 \ubd80\uc5b4 \uc794\uc744 \ucc44\uc6b4\ub2e4", "\uc794": "\ud1b0 \uce7c\ub9b0\uc988 \uae00\ub77c\uc2a4", "\uae30\ubc95": "\uc9c1\uc811\ub123\uae30(Build)"}, "img_src": "\ub9e5\uc8fc \ubca0\uc774\uc2a4_files/beerspri.jpg", "name": "\ube14\ub799 \ubca8\ubcb3(Black Velvet)"}, {"descs": {"\uc7ac\ub8cc": "\ud398\ud37c\ubbfc\ud2b8[15 ml],\ub9e5\uc8fc", "\ub9cc\ub4e4\uae30": "\ub0c9\uac01\uc2dc\ud0a8 \ub9e5\uc8fc\ub97c \ub530\ub974\uace0 \ud398\ud37c\ubbfc\ud2b8\ub97c \ucca8\uac00\ud574 \uac00\ubccd\uac8c \uc800\uc5b4\uc900\ub2e4", "\uc794": "\uace0\ube14\ub81b", "\uae30\ubc95": "\uc250\uc774\ud06c(Shake)"}, "img_src": "\ub9e5\uc8fc \ubca0\uc774\uc2a4_files/blank.gif", "name": "\ubbfc\ud2b8\ube44\uc5b4(Mint Beer)"}, {"descs": {"\uae30\ubc95": "\uc9c1\uc811\ub123\uae30(Build)", "\uc7ac\ub8cc": "\ub9e5\uc8fc (90 ml),\uc18c\ub2e4\uc218 [90 ml]", "\ub9cc\ub4e4\uae30": "\ucc28\uac8c \uc900\ube44\ud55c \uc7ac\ub8cc\ub97c \uc794\uc5d0 \ubd93\ub294\ub2e4", "\uc794": "8\uc628\uc2a4 \ud140\ube14\ub7ec(240ml)", "\uba54\ubaa8": "\uc54c\ucf5c\ub3c4\uc218\uac00 \ub0ae\uace0 \ub354\uc6b1 \uccad\ub7c9\uac10\uc744 \uc8fc\ub294 \ub9e5\uc8fc\uc758 \ub9db\uc744 \uc990\uae38 \uc218 \uc788\ub2e4.", "\ub9db": "\uc309\uc300\ud55c \ub9db, \uc62c\ub370\uc774 \ud0c0\uc785"}, "img_src": "\ub9e5\uc8fc \ubca0\uc774\uc2a4_files/blackvel.jpg", "name": "\ud30c\ub098\uc250(Panache)"}, {"descs": {"\uae30\ubc95": "\uc9c1\uc811\ub123\uae30(Build).", "\uc7ac\ub8cc": "\ub9e5\uc8fc [90 ml],\ud1a0\ub9c8\ud1a0\uc8fc\uc2a4 [90 ml]", "\ub9cc\ub4e4\uae30": "\ubbf8\ub9ac \ucda9\ubd84\ud788 \ub0c9\uac01\uc2dc\ud0a8 \uc7ac\ub8cc\ub97c \uc794\uc5d0 \ubd93\uace0 \uac00\ubccd\uac8c \uc813\ub294\ub2e4", "\uc794": "8\uc628\uc2a4 \uc2a4\ud15c\ub808\uc2a4 \ud544\uc2a4\ub108[240ml]", "\uc720\ub798": "\uc219\ucde8\ub85c \uc778\ud558\uc5ec \ub208\uc774 \ucda9\ud608\ub418\uc5c8\uc744 \ub54c \ub9c8\uc2dc\uba74 \ub180\ub78d\uac8c\ub3c4 \ucda9\ud608\ub418\uc5c8\ub358 \ub208\uc774 \uc815\uc0c1\uc73c\n\t\ub85c \ub3cc\uc544\uc628\ub2e4\uace0 \ud558\uc5ec \ubd99\uc5ec\uc9c4 \uc774\ub984\uc778\ub370 '\uc219\ucde8\uc758 \ubb18\uc57d'\uc774\ub77c\uace0 \ub110\ub9ac \uc54c\ub824\uc838 \uc788\ub2e4.", "\ub9db": "\uc309\uc300\ud55c \ud1a0\ub9c8\ud1a0 \uc8fc\uc2a4 \ub9db, \uc62c\ub370\uc774 \ud0c0\uc785"}, "img_src": "\ub9e5\uc8fc \ubca0\uc774\uc2a4_files/blank.gif", "name": "\ub808\ub4dc\uc544\uc774"}, {"descs": {"\uae30\ubc95": "\uc9c1\uc811\ub123\uae30[Build]", "\uc7ac\ub8cc": "\ub9e5\uc8fc [90 ml], \uc9c4\uc800\uc5d8 [90 ml]", "\ub9cc\ub4e4\uae30": "\ubbf8\ub9ac \ucc28\uac8c \ud55c \uc7ac\ub8cc\ub97c \uc794\uc5d0 \ubd93\ub294\ub2e4.", "\uc794": "8\uc628\uc2a4 \uc2a4\ud15c\ub808\uc2a4 \ud544\uc2a4\ub108", "\uc720\ub798": "\uc601\uad6d\uc758 \ud751\ub9e5\uc8fc\uc778 \uc2a4\ud0c0\uc6b0\ud2b8\uc640 \uc9c4\uc800\uc5d8\ub85c \ud63c\ud569\ud558\uc5ec \ub9c8\uc2dc\ub294\ub370 \uc601\uad6d\uc758 \ub9e5\uc8fc\ub294\n        \uace0\uc628\ubc1c\ud6a8\ub9e5\uc8fc\uc774\ubbc0\ub85c \uc57d\uac04 \ucc28\uac8c \ud558\uac70\ub098 \uadf8\ub0e5 \ub9c8\uc2dc\ub294\ub370 \ubbf8\uad6d\uc778\ub4e4\uc740 \ucc28\uac8c\ud558\uc5ec\n\t\ub9c8\uc2e0\ub2e4.", "\ub9db": "\uc57d\uac04 \ub2e8\ub9db\uc758 \ub9e5\uc8fc \ub9db, \uc62c\ub370\uc774 \ud0c0\uc785"}, "img_src": "\ub9e5\uc8fc \ubca0\uc774\uc2a4_files/mintbeer.jpg", "name": "\uc0cc\ub514 \uac1c\ud504(Shandy Gaff)"}]