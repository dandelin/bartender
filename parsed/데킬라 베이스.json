[{"descs": {"\uae30\ubc95": "\uc9c1\uc811\ub123\uae30(build)", "\uc7ac\ub8cc": "\ub370\ud0ac\ub77c [45 ml], \ud30c\uc778\uc560\ud50c\uc8fc\uc2a4 [120 ml], \uc124\ud0d5\uc2dc\ub7fd [2 Tsp]", "\ub9cc\ub4e4\uae30": "\uc794\uc5d0 \ub370\ud0ac\ub77c\uc640 \uc124\ud0d5\uc2dc\ub7fd,\ubbf8\ub9ac \ucc28\uac8c \uc900\ube44\ud55c \ud30c\uc778\uc560\ud50c\uc8fc\uc2a4\ub97c \ubd93\uace0 \uc798 \n\t\uc800\uc740 \ub2e4\uc74c \uc5bc\uc74c\uc744 \ub123\ub294\ub2e4.", "\uc794": "8\uc628\uc2a4 \ud544\uc2a4\ub108(240ml)", "\uba54\ubaa8": "\uc54c\ucf5c \ub9db\uc774 \uc57d\ud558\uba70 \ud30c\uc778\uc560\ud50c\uc8fc\uc2a4 \uc131\uaca9\uc758 \ubd80\ub4dc\ub7ec\uc6b4 \ub9db\uc788\ub2e4.", "\ub9db": "\ub2e8\ub9db, \uc2dd\ud6c4"}, "img_src": "\ub370\ud0ac\ub77c \ubca0\uc774\uc2a4_files/blank.gif", "name": "\uc790\ub77c\ub098(Jarana)"}, {"descs": {"\uae30\ubc95": "\uc9c1\uc811\ub123\uae30(build).", "\uc7ac\ub8cc": "\ub370\ud0ac\ub77c [45 ml], \ud2b8\ub9ac\ud50c \uc139 [10 ml], \ub808\ubaac\uc774\ub098 \ub77c\uc784 \uc8fc\uc2a4 [10 ml]", "\ub9cc\ub4e4\uae30": "\uce75\ud14c\uc77c \uae00\ub77c\uc2a4\uc758 \uac00\uc7a5\uc790\ub9ac\uc5d0 \ub808\ubaac\uc999\uc744 \uc801\uc2e0 \ud6c4 \uc18c\uae08\uc744 \ucc0d\uc5b4\uc11c \ub193\ub294\ub2e4.\n           \uc5bc\uc74c\uacfc \uc7ac\ub8cc\ub97c \ud568\uaed8 \ub123\uc740 \ud6c4 \uc798 \uc11e\uc5b4\uc11c \uc900\ube44\ub41c \uae00\ub77c\uc2a4\uc5d0 \ub530\ub978\ub2e4.", "\uba54\ubaa8": "\uc774 \uce75\ud14c\uc77c\uc740 1949\ub144 LA\uc758 \ud55c \ubc14\ud150\ub354\uc5d0 \uc758\ud574 \ubbf8\uad6d \uce75\ud14c\uc77c \ucf58\ud14c\uc2a4\ud2b8\uc5d0\uc11c\n\t\uc785\uc120\ub41c \uc791\ud488\uc774\ub2e4.", "\uc751\uc6a9": "\ud478\ub978\uc0c9\uc758 \ud2b8\ub9ac\ud50c \uc139\uc744 \uc4f0\uba74 [\ube14\ub8e8 \ub9c8\uac00\ub9ac\ud0c0(Blue Margarita)]\uac00 \ub41c\ub2e4.", "\uc720\ub798": "\uc0c8\ucf64\ud55c \ub9db\uc73c\ub85c \uc5ec\uc131\ub4e4\uc758 \uc778\uae30\ub97c \ub04c\uace0\uc788\ub294 \uce75\ud14c\uc77c \ub9c8\ub974\uac00\ub9ac\ud0c0\ub294\n\t\uc18c\uae08\uacfc \ub77c\uc784 \ub610\ub294 \ub808\ubaac\uacfc \ub370\ud0ac\ub77c\uc758 \uacb0\ud569\uc740 \uba55\uc2dc\ucf54\uc778\uc758 \uc804\ud1b5\uc801\uc778 \ub370\ud0ac\ub77c\n\t\ub9c8\uc2dc\ub294 \ubc95\uacfc\ub3c4 \ud1b5\ud55c\ub2e4. \uc0c8\ucf64 \ud558\uba74\uc11c\ub3c4 \uc0b0\ub73b\ud55c \ub9db \ub54c\ubb38\uc5d0 \uc5ec\uc131\uc5d0\uac8c \uc778\uae30\n\t\uc788\ub294 \uce75\ud14c\uc77c\uc774\ub2e4."}, "img_src": "\ub370\ud0ac\ub77c \ubca0\uc774\uc2a4_files/jarana.jpg", "name": "\ub9c8\uac00\ub9ac\ud0c0(Margarita)"}, {"descs": {"\uc7a5\uc2dd": "\ud30c\uc778\uc560\ud50c, \uccb4\ub9ac.", "\uc7ac\ub8cc": "\ub370\ud0ac\ub77c [30 ml], \ud30c\uc778\uc560\ud50c\uc8fc\uc2a4", "\ub9cc\ub4e4\uae30": "\ub0c9\uac01\ub41c \uc62c\ub4dc \ud328\uc158 \uae00\ub77c\uc2a4\uc5d0 \uc5bc\uc74c\uacfc \ud568\uaed8 \uc7ac\ub8cc\ub97c \ub530\ub974\uace0 \ud30c\uc778\uc560\ud50c\uacfc \ucc44\ub9ac\n\t   \ub85c \uc7a5\uc2dd\uc744 \ud55c\ub2e4.", "\uba54\ubaa8": "'\ud22c\uc6b0\uc0ac' \ub77c\ub294 \ub73b\uc758 \uce75\ud14c\uc77c\ub85c \uba55\uc2dc\ucf54 \ud2b9\uc0b0\ud488\uc778 \ub370\ud0ac\ub77c\uc758 \uae4a\uc740 \ub9db\uacfc \ud30c\uc778\uc560\n\t\ud50c\ud5a5\uc774 \uc798 \uc5b4\uc6b8\ub9ac\ub294 \uc74c\ub8cc\uc774\ub2e4.", "\uae30\ubc95": "\uc9c1\uc811\ub123\uae30(build)."}, "img_src": "\ub370\ud0ac\ub77c \ubca0\uc774\uc2a4_files/blank.gif", "name": "\ub9c8\ud0c0\ub3c4\ub974(Martador)"}, {"descs": {"\uc7ac\ub8cc": "\ub370\ud0ac\ub77c [30 ml], \ub77c\uc784 \uc8fc\uc2a4 [7.5 ml]", "\ub9cc\ub4e4\uae30": "\uc5bc\uc74c\uc774 \ub4e0 \ud558\uc774 \ubcfc \uae00\ub77c\uc2a4\uc5d0 \uc7ac\ub8cc\ub97c \ub123\uc740\ud6c4 \ucf5c\ub77c\ub85c \uc794\uc744 \ucc44\uc6b4\ub4a4 \uac00\ubccd\uac8c\n\t   \uc800\uc5b4 \uc900\ub2e4.", "\uc0c1\uc2dd": "\ub098\uc774\uc560\uac00\ub77c \ud3ed\ud3ec\n\t\uc601\ud654 '\ub098\uc774\uc560\uac00\ub77c'\ub85c \uc798 \uc54c\ub824\uc9c4 \uc774 \ud3ed\ud3ec\ub294 \uce90\ub098\ub2e4\uc640 \ubbf8\uad6d \ub450 \ub098\ub77c\uc5d0 \uac78\uccd0\n\t\uc788\uc73c\uba70 1\ubd84\uac04\uc758 \uc218\ub7c9\uc774 \uc57d370\ub9cc \ub9ac\ud130\ub85c \ub4dc\ub7fc\ud1b5\uc73c\ub85c 18,500\uac1c\ubd84\uc774\ub098 \ub41c\ub2e4.", "\uae30\ubc95": "\uc9c1\uc811\ub123\uae30(Build)."}, "img_src": "\ub370\ud0ac\ub77c \ubca0\uc774\uc2a4_files/margarit.jpg", "name": "\uba55\uc2dc\ucf5c\ub77c\ub2e4(Mesicolada)"}, {"descs": {"\uae30\ubc95": "\ud754\ub4e4\uae30(Shake).", "\uc7ac\ub8cc": "\ub370\ud0ac\ub77c [15 ml], \ud06c\ub9bc \ub4dc \uba58\ud2b8 \ud06c\ub9bc [10 ml], \ub808\ubaac\uc8fc\uc2a4 [15 ml]", "\ub9cc\ub4e4\uae30": "\uc5bc\uc74c\uacfc \uc7ac\ub8cc\ub97c \uc250\uc774\ucee4\uc5d0 \ub123\uace0 \uac00\ubccd\uac8c \ud754\ub4e4\uc5b4 \uc7a5\uc5d0 \ub530\ub978\ub2e4.", "\uc794": "3\uc628\uc2a4 \uce75\ud14c\uc77c(180ml).", "\uba54\ubaa8": "\uba55\uc2dc\ucf54\uc758 \uc0c8 \uc774\ub984\uc73c\ub85c \uc544\ub984\ub2e4\uc6b4 \ub179\uc0c9\uc744 \uc990\uae30\uba70 \ub9c8\uc2dc\ub294\ub370 \uc88b\uace0,\ud754\uce58\uc54a\uc740 \n\t\uc0c8 \uc774\ub984\uc758 \uce75\ud14c\uc77c\uc911\uc5d0 \ud558\ub098\uc774\ub2e4.", "\ub9db": "\ubc15\ud558\ud5a5\uc758 \ub9db, \uc2dd\ud6c4."}, "img_src": "\ub370\ud0ac\ub77c \ubca0\uc774\uc2a4_files/blank.gif", "name": "\ubaa8\ud0b9\ubc84\ub4dc(Mocking Bird)"}, {"descs": {"\uc7a5\uc2dd": "\ub808\ubaac.", "\uc7ac\ub8cc": "\ub370\ud0ac\ub77c [45 ml], \ud1a0\ub9c8\ud1a0\uc96c\uc2a4", "\ub9cc\ub4e4\uae30": "\ud140\ube14\ub7ec \uae00\ub77c\uc2a4\uc5d0 \uc5bc\uc74c\uacfc \uc7ac\ub8cc\ub97c \ub123\uace0 \ud1a0\ub9c8\ud1a0\uc8fc\uc2a4 \uc801\ub2f9\ub7c9\uc744 \ub123\uace0 \ud5a5\uc2e0\ub8cc\n\t   \uc57d\uac04\uc744 \ub123\uc740\ud6c4 \ud754\ub4e4\uc5b4\uc11c \ub9cc\ub4e0\ub2e4.", "\uc0c1\uc2dd": "\ube68\uac04\uc0c9\uc740 \uc545\ub9c8\ub97c \uc81c\uac70\ud558\ub294 \uc0c9\uae54\uc774\ub2e4.", "\uae30\ubc95": "\ud754\ub4e4\uae30(Shake)."}, "img_src": "\ub370\ud0ac\ub77c \ubca0\uc774\uc2a4_files/martador.jpg", "name": "\uc2a4\ud2b8\ub85c\ud56b(Straw Hot)"}, {"descs": {"\uae30\ubc95": "\uc9c1\uc811\ub123\uae30(build)", "\uc7ac\ub8cc": "\ub370\ud0ac\ub77c [40 ml], \ud2b8\ub9ac\ud50c \uc139 [30 ml], \ud06c\ub80c\ubca0\ub9ac\uc8fc\uc2a4 [90 ml]", "\uc7a5\uc2dd": "\uc2ac\ub77c\uc774\uc2a4 \ub808\ubaac, \uccb4\ub9ac, \uc2a4\ud2b8\ub85c.", "\ub9cc\ub4e4\uae30": "\uc5bc\uc74c\uc744 \ub123\uc740 \uc794\uc5d0 \uc7ac\ub8cc\ub97c \ub123\uace0 \uc798 \uc813\ub294\ub2e4.", "\uc794": "10\uc628\uc2a4 \ud544\uc2a4\ub108(300ml)", "\uba54\ubaa8": "\uc720\ub09c\ud788 \uba55\uc2dc\ucf54\uc758 \ud0dc\uc591\uc740 \ub728\uac81\ub2e4. \ub530\uac00\uc6b4 \ud587\ube5b\uacfc \uac74\uc870\ud55c \uae30\ud6c4, \uc0b4\uacb0\uc774 \ud0c0\ub294\n\t\ub4ef\ud55c \ub354\uc704\uc5d0 \uc54c\ub9de\ub294 \uc815\uc5f4\uc801\uc778 \uce75\ud14c\uc77c\uc774\uc9c0\ub9cc \uc54c\ucf5c\ub3c4\uc218\ub294 \ub0ae\uc740 \ud3b8\uc774\ub2e4.", "\ub9db": "\uc57d\uac04 \ub2e8\ub9db, \uc2dd\ud6c4, \ud2b8\ub85c\ud53c\uce7c."}, "img_src": "\ub370\ud0ac\ub77c \ubca0\uc774\uc2a4_files/blank.gif", "name": "\uc120\ubc88(Sun Burn)"}, {"descs": {"\uae30\ubc95": "\uc9c1\uc811\ub123\uae30(Build).", "\uc7ac\ub8cc": "\ub370\ud0ac\ub77c [15 ml], \ubcf4\ub4dc\uce74 [15 ml], \ub7fc [15 ml], \ud2b8\ub9ac\ud3f4 \uc139 [15 ml], \n \ub808\ubaac\uc8fc\uc2a4 [15 ml], \ucf5c\ub77c [30 ml]", "\uc7a5\uc2dd": "\uc2ac\ub77c\uc774\uc2a4 \ub808\ubaac, \uc2a4\ud2b8\ub85c.", "\ub9cc\ub4e4\uae30": "\uc794\uc5d0 \uc5bc\uc74c\uacfc \uc7ac\ub8cc\ub97c \ub123\uace0 \uc798 \uc813\ub294\ub2e4.", "\uc794": "10\uc628\uc2a4 \ubc1c\ub8ec(300ml).", "\uba54\ubaa8": "\ub871 \uc544\uc77c\ub79c\ub4dc \uc544\uc774\uc2a4 \ud2f0\uc5d0\uc11c \ubcc0\ud615\ub41c \uac83\uc73c\ub85c \ub370\ud0ac\ub77c\uc758 \ubb34\uac70\uc6b4 \ub9db\uc774 \ud2b9\uc9d5\uc774\ub2e4.", "\ub9db": "\uc57d\uac04 \uc4f4\ub9db, \uc62c\ub370\uc774 \ud0c0\uc785.", "\uc0c1\uc2dd": "\uc694\ub9ac\uc758 \uae30\ubcf8\uc801\uc778 \uc7ac\ub8cc\ub85c \uc601\uc5b4\ub85c\ub294 egg\uc774\uace0, \ud504\ub791\uc2a4\uc5b4\ub85c\ub294 uf, \ub3c5\uc77c\uc5b4\ub85c\ub294\n\tei,\uc774\ud0c8\ub9ac\uc544\uc5b4\ub85c\ub294 uovo,\uc2a4\ud398\uc778\uc5b4\ub85c\ub294 huevo\ub77c\uace0 \ud55c\ub2e4."}, "img_src": "\ub370\ud0ac\ub77c \ubca0\uc774\uc2a4_files/mesicloa.jpg", "name": "\ud14d\uc0ac\uc2a4 \ud2f0(Texas Tea)"}, {"descs": {"\uae30\ubc95": "\ubb3b\ud788\uae30(Rimmed)\uc640 \ud754\ub4e4\uae30(Shake).", "\uc7ac\ub8cc": "\ub370\ud0ac\ub77c [45 ml], \ube14\ub8e8 \ud034\ub77c\uc18c [15 ml], \ub77c\uc784\uc8fc\uc2a4 [15 ml]", "\uc7a5\uc2dd": "\uc18c\uae08\ubb3b\ud798.", "\ub9cc\ub4e4\uae30": "\uba3c\uc800 \uc794 \uac00\uc7a5\uc790\ub9ac\uc5d0 \uc18c\uae08\uc744 \ubb3b\ud600 \ub193\uc740 \ud6c4 \uc7ac\ub8cc\uc640 \uc5bc\uc74c\uc744 \uc250\uc774\ucee4\uc5d0 \ub123\uace0\n          \uac00\ubccd\uac8c \ud754\ub4e4\uc5b4\uc11c \uc18c\uae08\uc774 \ub5a8\uc5b4\uc9c0\uc9c0 \uc54a\ub3c4\ub85d \uc870\uc2ec\ud558\uc5ec \uc794\uc5d0 \ub530\ub978\ub2e4.", "\uc794": "5\uc628\uc2a4 \ucfe0\ud3ab\ud2b8 \uce75\ud14c\uc77c(150ml).", "\uba54\ubaa8": "\uc5ec\ub7ec\uac00\uc9c0 \uc6d0\uc2dc\uc801\uc778 \uc5d0\ud53c\uc18c\ub4dc\uac00 \uc788\uc9c0\ub9cc \uac00\uc7a5 \uc2e0\ube59\uc131\uc788\ub294 \uac83\uc740 \ub85c\uc2a4\uc5d4\uc824\ub9ac\n\t\uc2a4\uc758 \ud55c \ub808\uc2a4\ud1a0\ub791 \ubc14\ud150\ub354\uac00 \uccab\uc0ac\ub791\uc758 \uc5ec\uc778 \ub9c8\uac00\ub9ac\ud0c0\ub97c \uc78a\uc9c0 \ubabb\ud574 \ub9cc\ub4e4\uc5b4\n\t\ub0b8 \uce75\ud14c\uc77c\uc774\ub77c\uace0\ub3c4 \ud55c\ub2e4.", "\uc751\uc6a9": "\ube14\ub8e8 \ud034\ub77c\uc18c \ub300\uc2e0\uc5d0 \uc0c9\uc774 \uc5c6\ub294 \ud034\ub77c\uc18c\ub97c \uc0ac\uc6a9\ud55c \uac83\uc774 \uc6d0\ub798\uc758 [\ub9c8\uac00\ub9ac\ud0c0\n        (Margarita)]\uc774\uace0 \ud478\ub978 \uc0c9\uc758 \ube14\ub8e8 \ud034\ub77c\uc18c\ub97c \uc0ac\uc6a9\ud558\uba74 \ube14\ub8e8 \ub9c8\uac00\ub9ac\ud0c0\ub77c\uace0\n\t\ubd80\ub978\ub2e4. \ucd5c\uadfc\uc5d0\ub294 \uc798\uac8c \ubd80\uc21c \uc5bc\uc74c\uc744 \uc794\uc5d0 \ub9ce\uc774 \ub123\ub294 \uacbd\uc6b0\ub3c4 \uc788\ub2e4.", "\ub9db": "\uc9e0\ub9db. \uc2e0\ub9db. \uc911\uac04 \uc4f4\ub9db. \uc62c\ub370\uc774 \ud0c0\uc785", "\uc720\ub798": "\ud1a0\ub9c8\ud1a0 \uc96c\uc2a4\uc640 \uc7a5\uc2dd\uc774 \uac74\uac15\ud55c \uc774\ubbf8\uc9c0\uc758 \uce75\ud14c\uc77c \ube14\ub7ec\ub4dc \ub9e4\ub9ac\ub294 \ubcf4\ub4dc\uce74\uac00 \n\t\ud1a0\ub9c8\ud1a0 \uc96c\uc2a4\uc640 \uac19\uc740 \ub9db\uc774 \ub098\ub294 \uc2dd\uc804\uc6a9 \uce75\ud14c\uc77c \ube14\ub8e8\ub514\ub780 '\ud53c\uac00\ub098\ub294'\uc774\ub780 \n\t\ub73b\uc73c\ub85c 16\uc138\uae30\uc5d0 \uc601\uad6d\uc5d0\uc11c \uccad\uad50\ub3c4\ub97c \ubc15\ud574\ud558\uc5ec '\ud53c\uc758 \uba54\uc774\ub9ac'\ub77c \ubd88\ub9b0 \uba54\n\t\uc774\ub9ac \ud29c\ub354 \uc5ec\uc655\uc5d0\uac8c\uc11c \uc774 \uce75\ud14c\uc77c\uc774 \uc720\ub798\ud588\ub2e4\uace0 \ud55c\ub2e4. \n\t\uae08\uc8fc\ubc95 \uc2dc\ub300\uc758 \ubbf8\uad6d\uc5d0\uc11c \uc9c4\uacfc \ud1a0\ub9c8\ud1a0 \uc96c\uc2a4\ub85c \ub9cc\ub4e4\uc5b4\uc9c4 \ube0c\ub8e8\ub514 \uc0d8\uc774\ub77c\ub294 \n\t\uce75\ud14c\uc77c\uc774 \uae30\ucd08\uac00 \ub41c\ub2e4."}, "img_src": "\ub370\ud0ac\ub77c \ubca0\uc774\uc2a4_files/blank.gif", "name": "\ube14\ub8e8\ub9c8\uac00\ub9ac\ud0c0(Blue Margarita)"}, {"descs": {"\uae30\ubc95": "\uc9c1\uc811\ub123\uae30(build).", "\uc7ac\ub8cc": "\ub370\ud0ac\ub77c [30 ml], \uc624\ub80c\uc9c0\uc8fc\uc2a4 [90 ml], \uadf8\ub808\ub098\ub518 \uc2dc\ub7fd [15 ml]", "\uc7a5\uc2dd": "\uc624\ub80c\uc9c0\uc870\uac01, \uccb4\ub9ac.", "\ub9cc\ub4e4\uae30": "\uc0ac\uc6cc \uae00\ub77c\uc2a4\uc5d0 \uc5bc\uc74c\uc744 \ub123\uace0 \uc7ac\ub8cc\ub97c \ub123\uc740\ud6c4 \uadf8\ub808\ub098\ub518 \uc2dc\ub7fd\uc744 \uc0b4\uc9dd \ubd80\uc5b4\n\t   \uac00\ub77c \uc549\ud78c\ub2e4.", "\uba54\ubaa8": "\ud0dc\uc591\uc758 \ub098\ub77c \uba55\uc2dc\ucf54\uc5d0\uc11c \uc2dc\uc791\ub418\uc5c8\uc73c\uba70, \uadf8\ub808\ub098\ub518 \uc2dc\ub7fd\uc758 \ube44\uc911\uc744 \uc774\uc6a9\ud558\uc5ec\n\t\uc77c\ucd9c\uc758 \ubd84\uc704\uae30\ub97c \uc0b4\ub9b0 \uc791\ud488\uc774\ub2e4.", "\uc720\ub798": "\ub864\ub7ad \uc2a4\ud1a4\uc988\uac00 \uc560\uc6a9\ud55c \uc0c8\ubcbd\ub9de\uc774 \uce75\ud14c\uc77c \ub370\ud0ac\ub77c \uc120\ub77c\uc774\uc988\ub294 1970\ub144\ub300\uc5d0 \ub864\n\t\ub9c1 \uc2a4\ud134\uc988\uac00 \uba55\uc2dc\ucf54\ub97c \ubc29\ubb38\ud588\uc744 \ub54c \uc774 \uce75\ud14c\uc77c\uc744 \uc54c\uac8c\ub418\uc5b4 \uadf8 \ud6c4 \uac00\ub294 \uacf3\n\t\ub9c8\ub2e4 \uc774 \uce75\ud14c\uc77c\uc744 \uc560\uc6a9\ud588\ub2e4\uace0 \ud55c\ub2e4. \ub9c8\uce68 \ud14c\ud0ac\ub77c \ubca0\uc774\uc2a4 \uce75\ud14c\uc77c\uc774 \uc0c8\ubcbd\uc744\n\t\ub9de\uc774\ud588\uc744 \uc2dc\uae30\uc600\ub2e4. \uc544\uce68\ub180\uc758 \uc2dc\ub300 1960\ub144\ub300\ub9d0-70\ub144\ub300 \uce98\ub9ac\ud3ec\ub2c8\uc544 \uc80a\uc740 \uce35\n\t\uc5d0\uac8c \ub370\ud0ac\ub77c\uac00 \uc778\uae30\uc788\uc5c8\ub2e4. \uc774\uae00\uc2a4\uac00 '\ub370\ud0ac\ub77c \uc120\ub77c\uc774\uc988'\ub77c\uace0 \ud558\ub294 \uace1\uc744 \ub178\n\t\ub798\ud558\uc600\ub358 \uac83\ub3c4 70\ub144\ub300 \ucd08\uc600\ub2e4."}, "img_src": "\ub370\ud0ac\ub77c \ubca0\uc774\uc2a4_files/mocking.jpg", "name": "\ub370\ud0ac\ub77c \uc120\ub77c\uc774\uc988(Tequila Sunrise)"}]