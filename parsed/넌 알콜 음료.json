[{"descs": {"\uc7ac\ub8cc": "\ubc14\ub098\ub098 [1/2 \uac1c], \ud50c\ub808\uc778 \uc694\uad6c\ub974\ud2b8 [1 \ucef5], \ubb3c [1 \ud070\uc220]", "\ub9cc\ub4e4\uae30": "\ubc14\ub098\ub098\ub294 \uaecd\uc9c8\uc744 \ubc97\uae30\uace0 \uc584\ud30d\ud558\uac8c \uc370\uc5b4 \ubbf9\uc11c\uc5d0 \ub123\uace0 \uac08\uac70\ub098, \uccb4\uc5d0 \ubc2d\uccd0\n\t  \uc73c\uae6c \ub2e4\uc74c \uafc0.\ud50c\ub808\uc778\uc694\uad6c\ub974\ud2b8\ub97c \ub123\uc5b4 \uace0\ub8e8\uc11e\ub294\ub2e4.", "\uba54\ubaa8": "\uc694\uad6c\ub974\ud2b8\uc5d0\ub294 \ub2e8\ubc31\uc9c8.\uce7c\uc298.\ube44\ud0c0\ubbfcA\uac00 \ud48d\ubd80\ud558\uac8c \ud568\uc720\ub418\uc5b4 \uc788\ub2e4."}, "img_src": "\ub10c \uc54c\ucf5c \uc74c\ub8cc_files/blank.gif", "name": "\ubc14\ub098\ub098 \uc694\uad6c\ub974\ud2b8"}, {"descs": {"\uc7ac\ub8cc": "\ubc30 [2 \uac1c], \ud1b5\ud6c4\ucd94 [2\ud070\uc220], \uc0dd\uac15 [5 \ud1a8], \uc7a3 [1 \ud070\uc220], \uc124\ud0d5 [1 \ucef5], \ubb3c [10 \ucef5]", "\ub9cc\ub4e4\uae30": "\ubc30\ub294 \uae38\uc774\ub85c 8\ub4f1\ubd84\ud55c \ub2e4\uc74d \ub2e4\uc2dc \ubc18\uc73c\ub85c \uc798\ub77c \uc0bd\uac01\ud615 \ubaa8\uc591\uc73c\ub85c \uc36c \ub2e4\uc74c\n\t  \uc18d\uc744 \ud310\ud310\ud558\uac8c \ud30c\ub0b4\uace0 \uac00\uc7a5\uc790\ub9ac\ub294 \ub3d9\uadf8\uc2a4\ub984\ud558\uac8c \ub3cc\ub824 \uae4e\uace0 \uc5f0\ud55c \uc124\ud0d5\ubb3c\n\t  \uc5d0 \ub2f4\uac00\ub454\ub2e4."}, "img_src": "\ub10c \uc54c\ucf5c \uc74c\ub8cc_files/banana.jpg", "name": "\ubc30\uc219"}, {"descs": {"\uc751\uc6a9": "\uc0b6\uc740 \ubcf4\ub9ac\uc300\uc740 \ubd84\ud64d\uc0c9 \uc2dd\uc6a9\uc0c9\uc18c\ub85c \ubb3c\uc744 \ub4e4\uc774\uace0 \ubb3c\ub4e4\uc778 \ubcf4\ub9ac\uc300\uc5d0 \ub179\ub9d0\uac00\n\t  \ub8e8\ub97c \uc785\ud78c\ub2e4.", "\uc7ac\ub8cc": "\ubcf4\ub9ac\uc300 [1/2 \ucef5], \ub179\ub9d0\uac00\ub8e8 [1/2 \ucef5], \ubb3c [4 \ucef5], \uc124\ud0d5 [1/3 \ucef5],\n \uc7a3 [1 \ud070\uc220], \uc5bc\uc74c\uc870\uae08, \ubd84\ud64d\uc0c9 \uc2dd\uc6a9\uc0c9\uc18c", "\ub9cc\ub4e4\uae30": "\ubcf4\ub9ac\uc300\uc740 \ubb3c\uc744 \ub109\ub109\ud788\ubd93\uace0 \ud479\uc0b6\uc544 \ucc2c\ubb3c\uc5d0 \ube44\ubcbc\uac00\uba70 \ud5f9\uad6c\uc5b4 \ubbf8\ub048\ubbf8\ub048\ud55c\n\t  \uac83\uc744 \uc5c6\uc564 \ub2e4\uc74c \uccb4\uc5d0 \ubc2d\uccd0 \ubb3c\uae30\ub97c \ube80\ub2e4."}, "img_src": "\ub10c \uc54c\ucf5c \uc74c\ub8cc_files/blank.gif", "name": "\ubcf4\ub9ac\uc218\ub2e8"}, {"descs": {"\uc7ac\ub8cc": "\ud1b5\uc870\ub9bc\ubcf5\uc22d\uc544 [4 \ucabd], \ud0c4\uc0b0\uc218 [2 \ucef5], \ubc31\ud3ec\ub3c4\uc8fc [1 \ucef5],\n \ub538\uae30 [5 \uac1c],  \uade4 [5 \uac1c]", "\ub9cc\ub4e4\uae30": "1.\ubcf5\uc22d\uc544\ub294 \ud1b5\uc5d0\uc11c \uaebc\ub0b4\uc5b4 \uc2dc\ub7fd\uacfc \ud569\uaed8 \ubbf9\uc11c\uc5d0 \ub123\uace0 \uacf1\uac8c \uac08\uc544\ub193\ub294\ub2e4.", "\uba54\ubaa8": "2.\ub538\uae30\ub294 \uaf2d\uc9c0\ub97c \ub5bc\uace0 \uc18c\uae08\ubb3c\uc5d0 \uc0b4\uba70\uc2dc \uc53b\uc5b4 \ubb3c\uae30\ub97c \ube80 \ub2e4\uc74c \ub465\uae00\ub0a9\uc791\n\t    \ud558\uac8c \uc36c\ub2e4."}, "img_src": "\ub10c \uc54c\ucf5c \uc74c\ub8cc_files/basook.jpg", "name": "\ubcf5\uc22d\uc544 \ud380\uce58"}, {"descs": {"\uae30\ubc95": "\ubd88 \ubd99\uc774\uae30(flambe).", "\uc7ac\ub8cc": "\ub728\uac70\uc6b4 \ucee4\ud53c [1 \ucef5], \uac01\uc124\ud0d5 [1 \uac1c], \ube0c\ub79c\ub514 [1 tsp]", "\ub9cc\ub4e4\uae30": "\ucee4\ud53c\uc794\uc5d0 \ub728\uac70\uc6b4 \ucee4\ud53c\ub97c \ubd93\uace0 \uc794 \uc704\uc5d0 \ud2f0\uc2a4\ud47c\uc744 \uac78\uccd0 \uac01\uc124\ud0d5\uc744 \uc62c\ub824\ub193\uc740\n\t  \ub2e4\uc74c \ube0c\ub79c\ub514\ub97c \ubd93\uace0 \ubd88\uc744 \ubd99\uc778\ub2e4.", "\uc794": "\ucee4\ud53c\uc794 \uc138\ud2b8.", "\uba54\ubaa8": "\ube0c\ub79c\ub514\uc5d0 \ubd88\uc744 \ubd99\uc774\uba74 \ud30c\ub780 \ubd88\uaf42\uc774 \ud558\ub298\uac70\ub9ac\uba70 \ud0c0\uc624\ub974\uac8c \ub418\ub294\ub370 \uc774\ub54c \uc2e4\ub0b4\n\t\uc870\uba85\uc744 \ub0ae\ucd94\uba74 \ud658\uc0c1\uc801\uc778 \ubd84\uc704\uae30\uac00\ub41c\ub2e4.", "\ub9db": "\ube0c\ub79c\ub514\ud5a5\uc758 \ucee4\ud53c \ub9db, \uc2dd\ud6c4"}, "img_src": "\ub10c \uc54c\ucf5c \uc74c\ub8cc_files/blank.gif", "name": "\uce74\ud398\ub85c\uc584"}, {"descs": {"\uc7ac\ub8cc": "\uc0f4\ud398\uc778 [1 \ucef5], \uc124\ud0d5 [1 \ud070\uc220], \ub538\uae30 [2 \uac1c]", "\ub9cc\ub4e4\uae30": "\uc624\ubaa9\ud55c \uadf8\ub987\uc5d0 \uc0f4\ud398\uc778\uc744 \ubd93\uace0 \uc124\ud0d5\uc744 \ub123\uc5b4 \ub179\uc778 \ub2e4\uc74c \uae68\ub057\uc774 \uc53b\uc740 \ub538\uae30\ub97c\n\t  \ub193\uace0 \ub0c9\uc7a5\uace0\uc5d0 \ub193\uc5b4 \ucc28\uac8c \ub454\ub2e4."}, "img_src": "\ub10c \uc54c\ucf5c \uc74c\ub8cc_files/bori.jpg", "name": "\uc0f4\ud398\uc778 \ud504\ub808\uc2a4"}, {"descs": {"\uc7ac\ub8cc": "\ucf54\ucf54\uc544\uac00\ub8e8  [1 \ud070\uc220 ], \uc0dd\ud06c\ub9bc  [1/2 \ucef5 ]", "\ub9cc\ub4e4\uae30": "\uc0dd\uc218\uc5d0 \ucf54\ucf54\uc544 \uac00\ub8e8\ub97c \ub123\uace0 \uc791\uc740 \uac70\ud488\uae30\ub85c \uc798\ud480\uc5b4 \ucc28\uac8c \ub454\ub2e4.\n\t  \ubbf9\uc11c\uc5d0 \uc704\uc758 \uac83\uacfc \uc0dd\ud06c\ub9bc\uc744 \ub123\uace0 \uac78\uc8fd\ud558\uac8c \uac08\uc544 \uae00\ub77c\uc2a4\uc5d0 \ubd80\uc73c\uba74 \ub9db\uc788\ub294\n\t  \uc170\uc774\ud06c\uac00 \ub41c\ub2e4."}, "img_src": "\ub10c \uc54c\ucf5c \uc74c\ub8cc_files/blank.gif", "name": "\ucf54\ucf54\uc544\uc250\uc774\ud06c"}, {"descs": {"\uae30\ubc95": "\ube14\ub80c\ub4dc(Blend).", "\uc7ac\ub8cc": "\ucf54\ucf54\ub11b \uc8fc\uc2a4 [60 ml], \uc624\ub80c\uc9c0 \uc8fc\uc2a4 [30 ml], \ud30c\uc778\uc560\ud50c \uc8fc\uc2a4 [30 ml], \ub77c\uc784 \uc8fc\uc2a4 [10 ml]", "\uc7a5\uc2dd": "\uc2ac\ub77c\uc774\uc2a4 \ud30c\uc778\uc560\ud50c,\uccb4\ub9ac,\uc2a4\ud2b8\ub85c", "\ub9cc\ub4e4\uae30": "\ube14\ub80c\ub354\uc5d0 \uc798\uac8c \ubcf4\uc190 \uc5bc\uc74c\uacfc \uc7ac\ub8cc\ub97c \ubaa8\ub450 \ub123\uace0 \ud63c\ud569\ud558\uc5ec \uc794\uc5d0 \uc5bc\uc74c\uacfc\n\t  \ud568\uaed8 \uc62e\uaca8 \ubd07\ub294\ub2e4. \ube14\ub80c\ub354\uac00 \uc5c6\uc744 \uacbd\uc6b0\ub294 \uc7ac\ub8cc\ub97c \uc250\uc774\ud06c\uc5d0 \ub123\uace0 \ud754\ub4e4\uc5b4\n\t  \uc794\uc5d0 \ubd80\uc21c \uc5bc\uc74c\uc744 \ub123\uace0 \ub530\ub978\ub2e4.", "\uc794": "8\uc628\uc2a4 \ud30c\ub974\ud398(240ml).", "\uba54\ubaa8": "\uc2ac\ub77c\uc774\uc2a4(slice)\ub780 \uc587\uac8c \uc370\uc740 \uac83\uc73c\ub85c \ub808\ubaac\uc774\ub098 \uc624\ub80c\uc9c0,\ud0a4\uc704 \ub4f1\uc5d0 \uc774\uc6a9\ub41c\ub2e4.", "\ub9db": "\ucf54\ucf54\ub11b\uc758 \ub9db\uacfc \ud5a5\uc774 \uac15\ud55c \ub2e8\ub9db."}, "img_src": "\ub10c \uc54c\ucf5c \uc74c\ub8cc_files/bsa.jpg", "name": "\ucf54\ucf54\ub11b"}, {"descs": {"\uc7ac\ub8cc": "\ucee4\ud53c [1 \ud070\uc220], \ud50c\ub808\uc778 \uc694\uad6c\ub974\ud2b8 [1 \ucef5], \uafc0 [1 \ud070\uc220], \ubb3c [2 \ud070\uc220]", "\ub9cc\ub4e4\uae30": "\ub728\uac70\uc740 \ubb3c\uc5d0 \ucee4\ud53c\ub97c \ud0c0\uc11c \uafc0\uc744 \ub123\uace0 \uc2dd\ud78c \ub2e4\ub4ec \ud50c\ub808\uc778 \uc694\uad6c\ub974\ud2b8\ub97c \ub123\uc5b4\n\t  \uace0\ub8e8 \uc11e\ub294\ub2e4.", "\uba54\ubaa8": "\uc694\uad6c\ub974\ud2b8\uc5d0\ub294 \ub2e8\ubc31\uc9c8.\uce7c\uc298.\ube44\ud0c0\ubbfcA\uac00 \ud48d\ubd80\ud558\uac8c \ud568\uc720\ub418\uc5b4 \uc788\ub2e4."}, "img_src": "\ub10c \uc54c\ucf5c \uc74c\ub8cc_files/blank.gif", "name": "\ucee4\ud53c\uc694\uad6c\ub974\ud2b8"}, {"descs": {"\uc7ac\ub8cc": "\ub450\uc720 [1 \ucef5], \ub2ec\uac40\ub178\ub978\uc790 [1 \uac1c], \uafc0 [1 \ud070\uc220], \ub808\ubaac\uc999 \uc870\uae08", "\ub9cc\ub4e4\uae30": "\ub2ec\uac40\uc744 \uae68\ub728\ub824 \ud770\uc790\uc640 \ub178\ub978\uc790\ub97c \uc870\uc2ec\uc2a4\ub7fd\uac8c \ub098\ub208\ud6c4 \uadf8\ub987\uc5d0 \ub450\uc720\ub97c \ubd93\uace0\n\t  \ub2ec\uac40\ub178\ub978\uc790\uc640 \uafc0\uc744 \ub123\uace0 \uac70\ud488\uae30\ub85c \uace0\ub8e8 \ud63c\ud569\ud55c \ub2e4\uc74c \ub808\ubaac\uc999\uc744 \ub123\ub294\ub2e4.", "\uba54\ubaa8": "\ub450\uc720\ub294 \ub9c8\uc2dc\ub294 \ucf69\uac00\uacf5\ud488\uc73c\ub85c, \uc2dd\ubb3c\uc131 \uc9c0\ubc29\uc774\uae30 \ub54c\ubb38\uc5d0 \uc131\uc778\ubcd1\uc608\ubc29\uc5d0 \uc88b\uc740\n\t\uac74\uac15\uc74c\ub8cc\uc774\ub2e4."}, "img_src": "\ub10c \uc54c\ucf5c \uc74c\ub8cc_files/caferoy.jpg", "name": "\ub450\uc720 \uc170\uc774\ud06c"}, {"descs": {"\uc7ac\ub8cc": "\ud770\ucf69 [1 \ucef5], \uc124\ud0d5 [1 \uc791\uc740\uc220], \ub7fc [1 \uc791\uc740\uc220], \uc218\ubc15 \uc801\ub2f9\ub7c9", "\ub9cc\ub4e4\uae30": "\ud770\ucf69 1\ucef5\uc744 \uc798 \uc53b\uc5b4\uc11c \ud558\ub8fb\ubc24 \ubd88\ub838\ub2e4\uac00 \uc190\uc73c\ub85c \ube44\ubcbc \uaecd\uc9c8\uc744 \ub9d0\ub054\ud788 \ubc97\uae34\n\t  \ub2e4\uc74c \ubb3c\uc744 \ubd93\uace0 \uc0b6\ub294\ub2e4.", "\uba54\ubaa8": "\ucf69\uc774 \uc775\uc73c\uba74 \ucc2c\ubb3c\uc5d0 \ud5f9\uad88 \ubbf9\uc11c\uc5d0 \ub123\uace0 \uacf1\uac8c \uac08\uc740\n\t  \ub2e4\uc74c \uccb4\uc5d0 \uac70\ub7ec \ucc28\uac8c \uc2dd\ud78c\ub2e4."}, "img_src": "\ub10c \uc54c\ucf5c \uc74c\ub8cc_files/blank.gif", "name": "\ub450\uc720 \ud380\uce58"}, {"descs": {"\uc7ac\ub8cc": "\uc624\ub80c\uc9c0\uc8fc\uc2a4 [5 \ucef5], \ubc31\ud3ec\ub3c4\uc8fc [1/2 \ucef5], \ud1b5\uc870\ub9bc\ud30c\uc774\uc560\ud50c [2 \ucabd],\n \ud1b5\uc870\ub9bc\ubcf5\uc22d\uc544 [2 \ucabd], \ud0a4\uc704 [1 \uac1c], \uba5c\ub860 [1/4 \uac1c], \ubc14\ub098\ub098 [1/2 \uac1c], \ub538\uae30 [5 \uac1c], \uc0ac\uacfc [1/2 \uac1c]", "\ub9cc\ub4e4\uae30": "1.\ud0a4\uc704\ub294 \uaecd\ucc14\uc744 \ubc97\uae30\uace0 \ubc18\ub2ec\ubaa8\uc591\uc73c\ub85c \uc36c\ub2e4.", "\uba54\ubaa8": "2.\ubc14\ub098\ub098\ub294 \uaecd\uc9c8\uc744 \ubc97\uaca8 \uc584\ud30d\ud558\uac8c \uc36c\ub2e4. \ud30c\uc778\uc560\ud50c\uacfc \ubcf5\uc22d\uc544\ub294 \ud1b5\uc870\ub9bc\uc744\n\t    \uc4f0\uac70\ub098 \uc2e0\uc120\ud55c \uac83\uc73c\ub85c \uc900\ube44\ud558\uc5ec \uc584\ud30d\ud558\uac8c \uc36c\ub2e4. \uc0ac\uacfc\ub294 \uae68\ub057\uc774 \uc53b\uc5b4\n\t    \uaecd\uc9c8\uc744 \ubc97\uaca8\uc11c \uc370\uac70\ub098 \uaecd\uc9c8\ub85c \uc584\ud30d\ud558\uac8c \uc36c\ub2e4."}, "img_src": "\ub10c \uc54c\ucf5c \uc74c\ub8cc_files/champagn.jpg", "name": "\ud504\ub8e8\uce20\ud380\uce58"}, {"descs": {"\uc7ac\ub8cc": "\uc778\uc0bc [3 \ubfcc\ub9ac], \ub300\ucd94 [10 \uac1c], \ubb3c [5 \ucef5], \uafc0 [1 \uc791\uc740\uc220]", "\ub9cc\ub4e4\uae30": "\uc778\uc0bc\uc740 \uc798\uac8c \uc370\uc5b4 \uc53b\uc5b4\ub193\uace0, \ub300\ucd94\ub3c4 \ub9d0\ub054\ud788 \uc53b\uc5b4\ub193\ub294\ub2e4.\n\t  \uc8fc\uc804\uc790\ub098 \ub0c4\ube44\uc5d0 \ubb3c\uc744 \ubd93\uace0 \uc778\uc0bc\uacfc \ub300\ucd94\ub97c \ub123\uc5b4 2\uc2dc\uac04 \uc774\uc0c1 \ud479\ub053\uc778\ub2e4.\n\t  \ucda9\ubd84\ud788 \uc6b0\ub7ec\ub098\uba74 \ubd88\uc5d0\uc11c \ub0b4\ub824 \uace0\uc6b4\uccb4\ub098 \uac70\uc988\uc5d0 \uac78\ub7ec\ub0b8\ub2e4.", "\uba54\ubaa8": "\uc774 \uc778\uc0bc\ub300\ucd94\ucc28\ub97c \ucc28\uac11\uac8c \ub450\uc5c8\ub2e4\uac00 \uadf8\ub300\ub85c \ub9c8\uc2dc\uac70\ub098 \uc2dd\uc131\uc5d0 \ub530\ub77c \uafc0\uc744\n\t  \ud0c0\uc11c \ub9c8\uc154\ub3c4 \uc88b\ub2e4."}, "img_src": "\ub10c \uc54c\ucf5c \uc74c\ub8cc_files/blank.gif", "name": "\uc778\uc0bc\ub300\ucd94\ub0c9\ucc28"}, {"descs": {"\uae30\ubc95": "\ubb3b\ud788\uae30(rimmed with sugar)\uc640 \ubd88 \ubd99\uc774\uae30(flambe)", "\uc7ac\ub8cc": "\uc544\uc774\ub9ac\uc26c \uc704\uc2a4\ud0a4 [30 ml], \ud751\uc124\ud0d5 [1 Tsp], \ub728\uac70\uc6b4 \ucee4\ud53c [120 ml], \uac70\ud488\ub0b8 \uc0dd\ud06c\ub9bc [3 Tsp]", "\ub9cc\ub4e4\uae30": "\uc6b0\uc120 \uc0dd\ud06c\ub9bc\uc73c\ub85c \uac70\ud488\uc744 \ub0b4\uc5b4 \ub193\ub294\ub2e4. \uadf8\ub9ac\uace0 \uc794 \uac00\uc7a5\uc790\ub9ac\ub97c \ub808\ubaac\uc999\uc73c\ub85c\n\t  \uc801\uc154 \ud751\uc124\ud0d5\uc744 \ubb3b\ud78c \ub2e4\uc74c \uc794 \uc548\uc5d0 \uc544\uc774\ub9ac\uc26c \uc704\uc2a4\ud0a4\ub97c \ub123\uace0 \ub7a8\ud504\uc5d0\uc11c\n\t  \uc794\uc744 \ub370\uc6cc\uc11c \ubd88\uc744 \ubd99\uc778\ub2e4. \uac70\ud488\ub0b8 \uc0dd\ud06c\ub9bc\uc744 \ucee4\ud53c \uc704\uc5d0 \uc5b9\uc5b4 \ub744\uc6b4\ub2e4.", "\uc794": "6\uc628\uc2a4 \uc544\uc774\ub9ac\uc26c \ucee4\ud53c(180ml)", "\uba54\ubaa8": "2.\ube44\uc5d4\ub098 \ucee4\ud53c\uc5d0\uc11c \uc0dd\ud06c\ub9bc\uc744 \ub123\uae30 \uc804\uc5d0 \ucee4\ud53c \ub9ac\ud050\ub974\uc778 \uce7c\ub8e8\uc544\ub97c \uc801\ub2f9\ud788\n\t  \ub123\uace0 \uc0dd\ud06c\ub9bc\uc744 \ub123\uac8c \ub418\uba74 \uce74\ud398 \uce7c\ub8e8\uc544\uac00 \ub418\ub294\ub370 \uba55\uc2dc\uce78 \ucee4\ud53c\ub77c\uace0\ub3c4 \ud558\ub2e4.", "\uc751\uc6a9": "1.\ubcf4\ud1b5 \ucee4\ud53c\uc794\uc5d0 \uc220\uc744 \ub123\uc9c0 \uc54a\uace0 \ucee4\ud53c\uc704\uc5d0 \uac70\ud488\ub0b8 \uc0dd\ud06c\ub9bc\uc744 \uc5b9\uc73c\uba74 \ube44\uc5d4\ub098\n\t  \ucee4\ud53c", "\ub9db": "\ucee4\ud53c\ud5a5\uacfc \ub354\ubd88\uc5b4 \uc911\uac04 \ub2e8\ub9db,\uc2dd\ud6c4\uc5d0 \uc88b\ub2e4."}, "img_src": "\ub10c \uc54c\ucf5c \uc74c\ub8cc_files/cocoa.jpg", "name": "\uc544\uc544\ub9ac\uc26c\ucee4\ud53c"}, {"descs": {"\uc7ac\ub8cc": "\ud0a4\uc704 [1 \uac1c], \ud50c\ub808\uc778 \uc694\uad6c\ub974\ud2b8 [1 \ucef5], \uafc0 [1 \ud070\uc220]", "\ub9cc\ub4e4\uae30": "\ud0a4\uc704\ub294 \uaecd\uc9c8\uc744 \ubc97\uae30\uace0 \uc584\ud30d\ud558\uac8c\uc36c\ub2e4. \uc370\uc5b4\ub193\uc740 \ud0a4\uc704\ub97c \ubbf9\uc11c\uc5d0 \ub123\uace0 \uacf1\uac8c\n\t  \uac08\uc544 \uafc0\uacfc \ud568\uaed8 \ucc28\uac8c \ud55c\ud6c4 \ud50c\ub808\uc778 \uc694\uad6c\ub974\ud2b8\ub97c \ub123\uc5b4 \uc798 \uc11e\uc73c\uba74 \ub41c\ub2e4.", "\uba54\ubaa8": "\uc694\uad6c\ub974\ud2b8\uc5d0\ub294 \ub2e8\ubc31\uc9c8.\uce7c\uc298.\ube44\ud0c0\ubbfcA\uac00 \ud48d\ubd80\ud558\uac8c \ud568\uc720\ub418\uc5b4 \uc788\ub2e4."}, "img_src": "\ub10c \uc54c\ucf5c \uc74c\ub8cc_files/blank.gif", "name": "\ud0a4\uc704 \uc694\uad6c\ub974\ud2b8"}, {"descs": {"\uae30\ubc95": "\ud754\ub4e4\uae30(shake)", "\uc7ac\ub8cc": "\ub808\ubaac [1/2 \uac1c\uc815\ub3c4\uc758 \uc999], \uc124\ud0d5 \uc2dc\ub7fd [15 ml], \uc0dd\uc218 \uc801\ub2f9\ub7c9", "\uc7a5\uc2dd": "\uc999\uc744 \uc9dc\uace0 \ub09c \ub808\ubaac\uc744 \ubcf4\uae30 \uc88b\uac8c \uc370\uc5b4\uc11c \uc7a5\uc2dd\ud558\uac70\ub098 \uc794 \uc548\uc5d0 \ub123\uace0 \uc2a4\ud2b8\ub85c\ub97c\n\t\uaf42\ub294\ub2e4.", "\ub9cc\ub4e4\uae30": "\uc250\uc774\ucee4\uc5d0 \uc5bc\uc74c\uacfc \ub808\ubaac\uc999,\uc124\ud0d5 \uc2dc\ub7fd\uc744 \ub123\uace0 \ucc28\uac8c \ud754\ub4e0 \ub2e4\uc74c \uc5bc\uc74c\uacfc \ud568\uaed8\n\t  \uc794\uc5d0 \uc62e\uaca8 \ub0c9\uac01\ub41c \uc0dd\uc218\ub85c 8\ud560 \uc815\ub3c4 \ucc44\uc6cc \uac00\ubccd\uac8c \uc813\ub294\ub2e4.", "\uc794": "10\uc628\uc2a4\uce7c\ub9b0\uc2a4(300ml)", "\uc751\uc6a9": "\ub808\ubaac\uc999\uc744 \ub728\uac70\uc6b4 \ubb3c\uc5d0 \uc11e\uc740 \ub2e4\uc74c \uc124\ud0d5\uc744 \ub123\uace0 \uc813\ub294\ub2e4.", "\ub9db": "\uc9c4\ud55c \ub808\ubaac\ud5a5\uc758 \uc2e0\ub9db."}, "img_src": "\ub10c \uc54c\ucf5c \uc74c\ub8cc_files/coconut.jpg", "name": "\ub808\ubaac\uc5d0\uc774\ub4dc"}, {"descs": {"\uae30\ubc95": "\ud754\ub4e4\uae30(shake).", "\uc7ac\ub8cc": "\ub808\ubaac [1/2 \uac1c \uc815\ub3c4 \ub808\ubaac\uc999], \uc124\ud0d5 \uc2dc\ub7fd [30 ml], \uc18c\ub2e4\uc218 \uc801\ub2f9\ub7c9", "\uc7a5\uc2dd": "\uc999\uc744 \uc9dc\uace0\ub09c \ub808\ubaac\uc744 \uc7a5\uc2dd\ud558\uac70\ub098 \uc2ac\ub77c\uc774\uc2a4\ud558\uc5ec \ub123\uace0 \uc2a4\ud2b8\ub85c\ub97c \uaf42\ub294\ub2e4.", "\ub9cc\ub4e4\uae30": "\uc250\uc774\ucee4\uc5d0 \uc5bc\uc74c\uacfc \ub808\ubaac\uc999, \uc124\ud0d5\uc2dc\ub7fd\uc744 \ub123\uace0 \ud754\ub4e4\uc5b4\uc11c \uc5bc\uc74c\uacfc \ud568\uaed8 \uc794\uc5d0\n\t  \uc62e\uaca8 \ubd93\uace0 \uc18c\ub2e4\uc218\ub85c \ucc44\uc6cc \uac00\ubccd\uac8c \uc813\ub294\ub2e4.", "\uc794": "10\uc628\uc2a4 \uce7c\ub9b0\uc2a4(300ml).", "\uba54\ubaa8": "\uc2ac\ub77c\uc774\uc2a4(slice)\ub780 \uc587\uac8c \uc370\uc740 \uac83\uc73c\ub85c \ub808\ubaac\uc774\ub098 \uc624\ub80c\uc9c0,\ud0a4\uc704 \ub4f1\uc5d0 \uc774\uc6a9\ub41c\ub2e4.\n\t\uc54c\ucf5c\uc774 \uc548\ub4e4\uc5b4 \uc788\uc73c\ubbc0\ub85c \ub204\uad6c\ub77c\ub3c4 \uc990\uae38 \uc218 \uc788\uace0 \ud2b9\ud788 \ud558\uc808\uae30 \uac08\uc99d \ud574\uc18c\uc5d0\n\t\ub300\ub2e8\ud788 \uc88b\ub2e4.", "\ub9db": "\ub808\ubaac \uc5d0\uc774\ub4dc\uc640 \ub9db\uc740 \ubc14\uc2b7\ud558\ub098 \ud0c4\uc0b0\uc218\uac00 \ub4e4\uc5b4\uc788\uc5b4 \ub354\uc6b1 \uccad\ub7c9\uac10\uc744 \uc900\ub2e4."}, "img_src": "\ub10c \uc54c\ucf5c \uc74c\ub8cc_files/blank.gif", "name": "\ub808\ubaac\uc2a4\ucffc\uc2dc"}, {"descs": {"\uc7ac\ub8cc": "\ub808\ubaac [1/2 \uac1c], \ud0c4\uc0b0\uc218 [1/2 \ucef5], \uc124\ud0d5\uc2dc\ub7fd [1 \ud070\uc220], \uc5bc\uc74c \uc801\ub2f9\ub7c9", "\ub9cc\ub4e4\uae30": "\ub808\ubaac\uc740 \ubc18\uc73c\ub85c \uc790\ub974\uace0 \uc7a5\uc2dd\uc73c\ub85c \ub744\uc6b8 \ub808\ubaac\uc740 \uc587\uac8c \uc800\uba70\uc36c\ub2e4.\n\t  \ubc18\uc73c\ub85c \uc790\ub978 \ub808\ubaac\uc744 \ub808\ubaac\uc999\uae30\uc5d0 \uc5b9\uc5b4 \uc999\uc744 \uc9dc\uace0 \uc124\ud0d5\uc740 \uac19\uc740 \ubd84\ub7c9\uc758\n\t  \ubb3c\uc744 \uc11e\uc5b4 \ub053\uc5ec\uc11c \uc2dc\ub7fd\uc744 \ub9cc\ub4e4\uace0 \ucc28\uac8c \uc2dd\ud78c \ub2e4\uc74c\ubc18\uc73c\ub85c \uc790\ub978 \ub808\ubaac\uc999\uc5d0\n\t  \ub123\uace0 \uace0\ub8e8 \uc11e\ub294\ub2e4."}, "img_src": "\ub10c \uc54c\ucf5c \uc74c\ub8cc_files/coffee.jpg", "name": "\ub808\ubaac\uc2a4\ucffc\uc2dc2"}, {"descs": {"\uc7ac\ub8cc": "\ud64d\ucc28 [1 \ubd09\uc9c0], \ubb3c [2 \ucef5], \uc704\uc2a4\ud0a4 [1 \ud070\uc220], \ub808\ubaac [1/4 \uac1c], \n\uc124\ud0d5 [1 \ud070\uc220], \uc5bc\uc74c \uc870\uae08", "\ub9cc\ub4e4\uae30": "1.\ub808\ubaac\uc740 \uae68\ub057\uc774 \uc53b\uc5b4 \uc584\ud30d\ud558\uac8c \uc36c\ub2e4.", "\uba54\ubaa8": "2.\uc8fc\uc804\uc790\uc5d0 \ubb3c\uc744 \ubd93\uace0 \ub053\uc778\ub2e4. \ubb3c\uc774 \ud314\ud314 \ub053\uc774\uba74 \ubd88\uc5d0\uc11c \ub0b4\ub824 \uc870\uae08\n\t    \uc2dd\ud78c \ub2e4\uc74c \ud64d\ucc28\ub97c 1\ubd09\uc9c0 \ub2f4\uac00\uc11c \ucda9\ubd84\ud788 \uc6b0\ub824\ub0b8 \ub2e4\uc74c \ubd09\uc9c0\ub97c \uac74\uc838\n\t    \ub0b4\uace0 \uc2dd\ud78c\ub2e4. \ub053\uc5ec\uc11c \uc2dd\ud78c \ud64d\ucc28\uc5d0 \uc704\uc2a4\ud0a4\ub97c \ub123\uc740\ud6c4 \uc2dd\uc131\uc5d0 \ub9de\ucd94\uc5b4\n\t    \uc124\ud0d5\uc744 \ub123\ub294\ub2e4. \ud64d\ucc28\uc5d0 \ub808\ubaac\uc744 \ub744\uc6b0\uace0 \uc5bc\uc74c\ub3c4 \ub744\uc6cc \ub0b4\uba74 \ub41c\ub2e4."}, "img_src": "\ub10c \uc54c\ucf5c \uc74c\ub8cc_files/blank.gif", "name": "\ub808\ubaac\ud2f0"}, {"descs": {"\uc7ac\ub8cc": "\uba5c\ub860 [1/6 \uac1c], \uc0dd\ud06c\ub9bc [1/2 \ucef5], \uc0dd\uc218 \uc870\uae08", "\ub9cc\ub4e4\uae30": "\uba5c\ub860\uc740 \uc528\ub97c \ube7c\uace0 \uaecd\uc9c8\uc744 \ubc97\uaca8 \uc584\ud30d\ud558\uac8c \uc370\uc5b4 \ucc28\uac8c\ub454\ub2e4."}, "img_src": "\ub10c \uc54c\ucf5c \uc74c\ub8cc_files/dowoo.jpg", "name": "\uba5c\ub860\uc250\uc774\ud06c"}, {"descs": {"\uc751\uc6a9": "\uc218\ubc15\uc740 1cm\ud06c\uae30\ub85c \uae4d\ub451\uc370\uae30\ud558\uace0 \uc528\ub97c \ube7c\ub0b8\ub2e4.\n\t  \ud0a4\uc704\uc640 \uc0ac\uacfc\ub294 \uaecd\uc9c8\uc744 \ubc97\uae30\uace0 1cm\ud06c\uae30\ub85c \uc798\uac8c \uc370\uace0 \ud1b5\uc870\ub9bd\ud30c\uc778\uc560\ud50c\ub3c4\n\t  \uc798\uac8c \uc36c\ub2e4. \uadf8\ub7f0 \ub2e4\uc74c \ubbf8\uc22b\uac00\ub8e8\uc5d0 \uc798\uac8c\uc36c \uacfc\uc77c\uacfc \uc5bc\uc74c\uc744 \ub744\uc6cc \ub0b8\ub2e4.", "\uc7ac\ub8cc": "\ubbf8\uc22b\uac00\ub8e8 [1/2 \ucef5], \uc124\ud0d5 [1/2 \ucef5], \ubb3c [4 \ucef5], \uc218\ubc15 [1/6 \uac1c], \n \ud0a4\uc704 [1 \uac1c], \uc0ac\uacfc [1/2 \uac1c], \ud1b5\uc870\ub9bc\ud30c\uc778\uc560\ud50c  [1 \ucabd]", "\ub9cc\ub4e4\uae30": "\ub053\uc5ec\uc11c \uc2dd\ud78c\ubb3c\uc5d0 \ubbf8\uc22b\uac00\ub8e8\ub97c \uc870\uae08\uc529 \ub123\uc5b4\uac00\uba70 \uba4d\uc6b8\uc774 \uc9c0\uc9c0 \uc54a\ub3c4\ub85d \uc798\n\t  \ud480\uace0 \ubbf8\uc22b\uac00\ub8e8\uc5d0 \uc124\ud0d5\uc744 \ub123\uc5b4 \ub2e8\ub9db\uc744 \ub0b8\ub2e4."}, "img_src": "\ub10c \uc54c\ucf5c \uc74c\ub8cc_files/blank.gif", "name": "\ubbf8\uc22b\uac00\ub8e8\ud654\ucc44"}, {"descs": {"\uc7ac\ub8cc": "\uc624\ub80c\uc9c0 [1/2 \uac1c], \ud0c4\uc0b0\uc218 [1/4 \ucef5], \uc544\uc774\uc2a4\ud06c\ub9bc \uc801\ub2f9\ub7c9", "\ub9cc\ub4e4\uae30": "\uc624\ub80c\uc9c0 1\uac1c\ub294 \ubc18\uc73c\ub85c \uc798\ub77c \uc999\uc744 \uc9e0 \ub2e4\uc74c \ucc28\uac8c\ub450\uc5c8\ub2e4\uac00 \uae00\ub77c\uc2a4\uc5d0 \ub2f4\ub294\ub2e4.\n\t  \uc624\ub80c\uc9c01/2\uac1c\ub294 \uaecd\uc9c8\uc744 \ubc97\uae30\uace0 \ubc18\ub2ec\ubaa8\uc591\uc73c\ub85c \uc36c\ud6c4 \uae00\ub77c\uc2a4\uc5d0 \ud0c4\uc0b0\uc218\ub97c\n\t  \ubd80\uc5b4 \uc624\ub80c\uc9c0\uc999\uacfc \uc798 \uc11e\uace0 \uc370\uc5b4\ub193\uc740 \uc624\ub80c\uc9c0\ub97c \ub193\uace0 \uc704\uc5d0 \uc544\uc774\uc2a4\ud06c\ub9bc\uc744\n\t  \uc5b9\ub294\ub2e4."}, "img_src": "\ub10c \uc54c\ucf5c \uc74c\ub8cc_files/dowoofun.jpg", "name": "\uc624\ub80c\uc9c0 \ud06c\ub9bc\uc18c\ub2e4"}, {"descs": {"\uc7ac\ub8cc": "\ud398\ud37c\ubbfc\ud2b8 [1 \ud070\uc220], \ud0c4\uc0b0\uc218 [2/3 \ucef5], \ub808\ubaac [1 \ucabd], \ub808\ubaac\uc999 \uc870\uae08, \uc5bc\uc74c \uc801\ub2f9\ub7c9", "\ub9cc\ub4e4\uae30": "\uae00\ub77c\uc2a4\uc5d0 \ud398\ud37c\ubbfc\ud2b8 1\ud070\uc220\uacfc \ub808\ubaac\uc999\uc744 \uc870\uae08 \ub123\uace0s \uc11e\uc740 \ub2e4\uc74c \ud0c4\uc0b0\uc218\ub97c\n\t  \ubd80\uc5b4 2~3\ubc88 \uac00\ubccd\uac8c \uc813\ub294\ub2e4. \ub808\ubaac\uacfc \uc5bc\uc74c\uc744 \ub744\uc6cc\ub0b8\ub2e4."}, "img_src": "\ub10c \uc54c\ucf5c \uc74c\ub8cc_files/blank.gif", "name": "\ud398\ud37c\ubbfc\ud2b8 \uc2a4\ucffc\uc2dc"}, {"descs": {"\uc7ac\ub8cc": "\ud1b5\uc870\ub9bc\ud30c\uc778\uc560\ud50c [1 \ucabd],  \ud30c\uc778\uc560\ud50c\uc8fc\uc2a4 [1/3 \ucef5], \ud30c\uc778\uc560\ud50c [1 \ucabd],\n \uccb4\ub9ac [3 \uac1c],  \uc544\uc774\uc2a4\ud06c\ub9bc \uc801\ub2f9\ub7c9", "\ub9cc\ub4e4\uae30": "\ud1b5\uc870\ub9bc \ud30c\uc778\uc560\ud50c\uacfc \ud30c\uc778\uc560\ud50c\uc8fc\uc2a4\ub294 \ubbf9\uc11c\uc5d0 \ub123\uc5b4 \uacf1\uac8c\uac08\uc544 \uac78\uc8fd\ud55c \ud30c\uc778\n\t  \uc560\ud50c\uc999\uc744 \ub9cc\ub4e0\ud6c4 \uc0dd\ud30c\uc778\uc560\ud50c\uc740 \uba39\uae30\uc88b\uc740 \ud06c\uae30\ub85c \uc36c\ub2e4."}, "img_src": "\ub10c \uc54c\ucf5c \uc74c\ub8cc_files/fruitpun.jpg", "name": "\ud30c\uc778\uc560\ud50c \ud50c\ub85c\ud2b8"}, {"descs": {"\uae30\ubc95": "\uc9c1\uc811 \ub123\uae30(Build).", "\uc7ac\ub8cc": "\ud06c\ub79c\ubca8\ub9ac\uc8fc\uc2a4 [60 ml], \uc0ac\uacfc \uc8fc\uc2a4 [30 ml], \ub77c\uc784 \uc8fc\uc2a4 [30 ml], \uc124\ud0d5 \uc2dc\ub7fd [15 ml]", "\uc7a5\uc2dd": "\uc2ac\ub77c\uc774\uc2a4 \uc624\ub80c\uc9c0, \uccb4\ub9ac, \uc2a4\ud2b8\ub85c.", "\ub9cc\ub4e4\uae30": "\uc794\uc5d0 \uc5bc\uc74c\uc744 \ub123\uc740 \ub2e4\uc74c \uc7ac\ub8cc\ub97c \ubaa8\ub450 \ub123\uace0 \uc798 \uc813\ub294\ub2e4.", "\uc794": "10\uc628\uc2a4 \ud544\uc2a4\ub108(300ml).", "\uba54\ubaa8": "\uc220\uc744 \ub123\uc9c0 \uc54a\uace0\ub3c4 \uce75\ud14c\uc77c \ubd84\uc704\uae30\ub97c \uc5f0\ucd9c\ud558\uc5ec \ub208\uc73c\ub85c \uc990\uae30\uba70 \ub9c8\uc2dc\ub294 \uc74c\ub8cc\uc774\ub2e4.", "\ub9db": "\uc138\uac00\uc9c0 \uacfc\uc77c \ub9db\uc774 \uc808 \uc5b4\uc6b8\ub9b0 \ub2e8\ub9db."}, "img_src": "\ub10c \uc54c\ucf5c \uc74c\ub8cc_files/blank.gif", "name": "\uc528\ube0c\ub9ac\uc988"}, {"descs": {"\uc7ac\ub8cc": "\uc218\ubc15 [1/2 \ud1b5], \uba54\ub860 [1 \uac1c], \ubc31\ud3ec\ub3c4\uc8fc [1/2 \ucef5], \uc124\ud0d5 [1/3 \ucef5], \ubb3c [4 \ucef5]", "\ub9cc\ub4e4\uae30": "1.\uc218\ubc15\uc740 \ubc18\uc744 \uac08\ub77c \uc791\uc740 \uc2a4\ucf65\uc774\ub098 \uc21f\uac00\ub77d\uc73c\ub85c \ub465\uae00\uac8c \ub5a0\ub0b8\ub2e4\uc74c \uc528\ub97c\n\t  \ube7c\ub0b8\ub2e4. \uba5c\ub860\ub4dc \ubc18\uc744 \uac08\ub77c \uc528\ub97c \ube7c\ub0b4\uace0 \ub465\uae00\uac8c \ub5a0\ub0b8\ub2e4.", "\uba54\ubaa8": "2.\ub0c4\ube44\uc5d0 \uc124\ud0d51/2\ucef5\uacfc \ubd88 4\ucef5\uc744 \ub123\uc5b4 \uc813\uc9c0 \ub9d0\uace0 \uc124\ud0d5\uc774 \ub179\uc744 \ub54c\uac00\uc9c0\n\t    \uc57d\ud55c\ubd88\uc5d0\uc11c \ub053\uc5ec \uc124\ud0d5\uc2dc\ub7fd\uc744 \ub9cc\ub4e4\uc5b4 \ucc28\uac8c \uc2dd\ud78c\ub2e4."}, "img_src": "\ub10c \uc54c\ucf5c \uc74c\ub8cc_files/insam.jpg", "name": "\uc218\ubc15\uba5c\ub860\ud380\uce58"}, {"descs": {"\uc7ac\ub8cc": "\uc218\uc0bc [1 \ubfcc\ub9ac], \uc0ac\uacfc [1/4 \uac1c],  \uafc0  [1 \ud070\uc220]", "\ub9cc\ub4e4\uae30": "\uc218\uc0bc\uc740 \uc194\ub85c \ube44\ubcbc \uae68\ub057\uc774 \uc53b\uace0, \uc218\uc0bc\uc740 \uc5b4\uc2b7\ud558\uac8c \uc370\uace0 \uc0ac\uacfc\ub294 \uaecd\uc9c8\uc744\n\t  \ubc97\uaca8 \uc528\ub97c \ube7c\uace0 \uc584\ud30d\ud558\uac8c \uc36c\ub2e4.", "\uba54\ubaa8": "\ubbf9\uc11c\uc5d0 \uc218\uc0bc.\uc0ac\uacfc.\uafc0\uc744 \ub123\uace0 \uacf1\uac8c \uac08\uc544 \uc218\uc0bc\uc0dd\uc999\uc744 \ub9cc\ub4e0\ub2e4."}, "img_src": "\ub10c \uc54c\ucf5c \uc74c\ub8cc_files/blank.gif", "name": "\uc218\uc0bc\uc0dd\uc999"}, {"descs": {"\uc7ac\ub8cc": "\ub538\uae30 [5 \uac1c], \uc124\ud0d5 [1 \ud070\uc220], \uc0dd\ud06c\ub9bc [1/2 \ucef5]", "\ub9cc\ub4e4\uae30": "\ub538\uae30\ub294 \uaf2d\uc9c0\ub97c \ub5bc\uace0 \uc5f0\ud55c \uc18c\uae08\ubb3c\uc5d0 \ud754\ub4e4\uc5b4 \uc53b\ub294\ub2e4.\n\t  \ubbf9\uc11c\uc5d0 \ub538\uae30\uc640 \uc124\ud0d5\uc744 \ub123\uace0 \uacf1\uac8c\uac04\ud6c4 \uc0dd\ud06c\ub9bc\uc744 \ub123\uace0 \ub2e4\uc2dc \uac78\uc8fd\ud558\uac8c \uac08\uc544\n\t  \uae00\ub77c\uc2a4\uc5d0 \ubd80\uc5b4\ub0b8\ub2e4."}, "img_src": "\ub10c \uc54c\ucf5c \uc74c\ub8cc_files/irishcf.jpg", "name": "\ub538\uae30\uc250\uc774\ud06c"}, {"descs": {"\uae30\ubc95": "\uc9c1\uc811 \ub123\uae30(Build).", "\uc7ac\ub8cc": "\ud30c\uc778\uc560\ud50c \uc8fc\uc2a4 [60 ml], \uc624\ub80c\uc9c0 \uc8fc\uc2a4 [30 ml], \ub808\ubaac \uc8fc\uc2a4 [30 ml], \uadf8\ub808\ub098\ub518\uc2dc\ub7fd [15 ml]", "\uc7a5\uc2dd": "\uc2ac\ub77c\uc774\uc2a4 \uc624\ub80c\uc9c0,\uccb4\ub9ac,\uc2a4\ud2b8\ub85c", "\ub9cc\ub4e4\uae30": "\uc794\uc5d0 \uc5bc\uc74c\uacfc \uc7ac\ub8cc\ub97c \ubaa8\ub450 \ub193\uace0 \uac00\ubccd\uac8c \uc800\uc740 \ub2e4\uc74c \uadf8\ub808\ub098\ub518\uc2dc\ub7fd\uc744 \uc870\uc2ec\n\t  \uc2a4\ub808 \ucc9c\ucc9c\ud788 \uc704\uc5d0\uc11c \ubd93\ub294\ub2e4. \uadf8\ub808\ub098\ub518\uc2dc\ub7fd\uc740 \uc8fc\uc2a4\ubcf4\ub2e4 \ubb34\uac81\uae30 \ub54c\ubb38\uc5d0\n\t  \uac00\ub77c\uc549\uac8c \ub41c\ub2e4.", "\uc794": "10\uc628\uc2a4 \ud544\uc2a4\ub108(300ml).", "\uba54\ubaa8": "\ud2b8\ub85c\ud53c\uce7c \ud615\ud0dc\ub85c \ub9cc\ub4e4\uc5b4\uc838 \uc788\uc5b4\uc11c \ud654\ub824\ud55c \uce75\ud14c\uc77c\ub85c \uc624\ud574\ud558\uae30 \uc26c\uc6b0\ub098\n\t\uc54c\ucf5c\uc740 \uc804\ud600 \ub4e4\uc5b4 \uc788\uc9c0 \uc54a\ub2e4.", "\ub9db": "\uc624\ub80c\uc9c0 \uc8fc\uc2a4\uc640 \ud30c\uc778\uc560\ud50c \uc8fc\uc2a4\uac00 \ud63c\ud569\ub41c \ub2e8\ub9db."}, "img_src": "\ub10c \uc54c\ucf5c \uc74c\ub8cc_files/blank.gif", "name": "\uc120\uc0e4\uc778"}, {"descs": {"\uae30\ubc95": "\uc9c1\uc811 \ub123\uae30(build).", "\uc7ac\ub8cc": "\ub77c\uc774\ud2b8\ub7fc [60 ml], \ud2b8\ub9ac\ud50c \uc139 [30 ml], \uc624\ub80c\uc9c0 \uc8fc\uc2a4 [60 ml], \ud30c\uc778\uc560\ud50c", "\uc7a5\uc2dd": "\uc885\ub958\uc5d0 \uad6c\uc560\uc5c6\uc774 \uc788\ub294 \uacfc\uc77c\uc744 \ubaa8\ub450 \uc608\uc058\uac8c \uc370\uc5b4 \ub9ce\uc774 \uc7a5\uc2dd\ud558\uace0 \uc2a4\ud2b8\ub85c\ub97c\n\t2\uac1c \uaf42\ub294\ub2e4.", "\ub9cc\ub4e4\uae30": "\uba3c\uc800 \ud30c\uc778\uc560\ud50c\uc744 \uc798\ub77c\uc11c \uc18d\uc744 \ud30c\ub0b8\ub2e4. \uc5bc\uc74c\uacfc \uc7ac\ub8cc\ub97c \ub123\uace0 \uc798 \uc11e\ub294\ub2e4.", "\uc794": "\ud30c\uc778\uc560\ud50c\uc794.", "\uba54\ubaa8": "\uc5f0\uc778\uc774 \ud568\uaed8 \ub9cc\ub4e4\uc5b4 \uc2e0\uc120\ud55c \uacfc\uc77c\uacfc \uc8fc\uc2a4\ub97c \ub3d9\uc2dc\uc5d0 \uc74c\ubbf8\ud558\uc5ec \uc624\ubd93\ud55c \uc2dc\uac04\uc744\n\t\uc990\uae30\uae30\uc5d0 \ub9e4\uc6b0 \uc88b\uc740 \uce75\ud14c\uc77c\uc774\ub2e4.", "\ub9db": "\ub2ec\ucf64\ud55c \uacfc\uc77c \ub9db, \uc2dd\ud6c4\uc5d0 \uc88b\ub2e4."}, "img_src": "\ub10c \uc54c\ucf5c \uc74c\ub8cc_files/kiwi.jpg", "name": "\uc6b0\ub9ac\ud568\uaed8"}, {"descs": {"\uc7ac\ub8cc": "\uc720\uc790 [2 \uac1c],  \ubc30 [1 \uac1c], \uc7a3 1 \ud070\uc220, \uc124\ud0d5(\ub610\ub294 \uafc0)\uc801\ub2f9\ub7c9, \uc124\ud0d5\uc2dc\ub7fd", "\ub9cc\ub4e4\uae30": "\uc720\uc790\ub294 \uaecd\uc9c8\uc9f8 \uae68\ub057\uc774 \uc53b\uc5b4 \ubb3c\uae30\ub97c \ub2e6\uace0 \uae38\uc774\uace0 4\ub4f1\ubd84 \ud558\uc5ec \uaecd\uc9c8\uc744 \ubc97\uae34\n\t  \ub2e4\uc74c \uc54c\ub9f9\uc774\ub97c \ud558\ub098\uc529 \ub5bc\uc5b4 \uc528\ub97c \ube7c\ub0b8\ub2e4."}, "img_src": "\ub10c \uc54c\ucf5c \uc74c\ub8cc_files/blank.gif", "name": "\uc720\uc790\ud654\ucc44"}, {"descs": {"\uae30\ubc95": "\uc9c1\uc811 \ub123\uae30(build)", "\uc7ac\ub8cc": "\ud1a0\ub9c8\ud1a0 \uc8fc\uc2a4", "\uc7a5\uc2dd": "\ucabc\uac20 \ub808\ubaac(wedge).", "\ub9cc\ub4e4\uae30": "\uc794\uc5d0 \uc5bc\uc74c\uacfc \ud1a0\ub9c8\ud1a0 \uc8fc\uc2a4\ub97c 8\ud560\uc815\ub3c4 \ucc44\uc6b4 \ud6c4 \uc18c\uae08, \ud6c4\ucd94, \ud56b\uc18c\uc2a4\ub97c\n\t  \uc57d\uac04 \ucca8\uac00\ud558\uc5ec  \uc798\uc813\ub294\ub2e4.", "\uc794": "8\uc628\uc2a4 \ud558\uc774\ube14(240ml)", "\uba54\ubaa8": "\ud1a0\ub9c8\ud1a0 \uc8fc\uc2a4\uc5d0 \uba87\uac00\uc9c0 \uc591\ub150\uc744 \ub123\uc740 \uac74\uac15\uc2dd\ud488 \uac19\uc740 \uc74c\ub8cc\uc778\ub370 \ub808\ubaac\uc999\uc744 \uc9dc\n\t\ub123\uc73c\uba74 \ub354\uc6b1 \ud5a5\uae30\ub86d\ub2e4.", "\uc751\uc6a9": "\ubc84\uc9c4 \uba54\ub9ac\uc5d0 \ubcf4\ub4dc\uce74\ub97c \ub123\uc73c\uba74 \ube14\ub7ec\ub514 \uba54\ub9ac(bloody mary)\uac00 \ub41c\ub2e4."}, "img_src": "\ub10c \uc54c\ucf5c \uc74c\ub8cc_files/lemonade.jpg", "name": "\ubc84\uc9c4\uba54\ub9ac"}, {"descs": {"\uc7ac\ub8cc": "\ud50c\ub808\uc778\uc694\uad6c\ub974\ud2b8 [1/2 \ucef5], \ud0c4\uc0b0\uc218 [1/2 \ucef5], \uafc0 [1 \uc791\uc740\uc220], \uc5bc\uc74c \uc801\ub2f9\ub7c9", "\ub9cc\ub4e4\uae30": "\ucc28\uac8c \uc2dd\ud78c \ud50c\ub808\uc778 \uc694\uad6c\ub974\ud2b8\ub97c \uae00\ub77c\uc2a4\uc5d0 \ub2f4\uace0 \ud0c4\uc0b0\uc218\ub97c \ub123\uc5b4 \uc798 \uc800\uc740\n\t  \ub2e4\uc74c \uc5bc\uc74c\uc744 \ub744\uc6b4\ub2e4."}, "img_src": "\ub10c \uc54c\ucf5c \uc74c\ub8cc_files/blank.gif", "name": "\uc694\uad6c\ub974\ud2b8\uc2a4\ucffc\uc2dc"}]