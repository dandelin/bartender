[{"descs": {"\uc7ac\ub8cc": "\ube0c\ub79c\ub514 [30 ml], \ud06c\ub9bc \ub4dc \uce74\uce74\uc624 [40 ml],  \uc2a4\uc704\ud2b8 \ud06c\ub9bc [70 ml], \ub108\ud2b8 \ub9e5", "\ub9cc\ub4dc\ub294 \ubc29\ubc95": "\uc250\uc774\ucee4\uc5d0 \ub108\ud2b8 \ub9e5\uc744 \uc81c\uc678\ud55c \uc7ac\ub8cc\ub97c \uc5bc\uc74c\uacfc \ud568\uaed8 \uc798 \uc11e\uc5b4 \ub0c9\uac01\ub41c \uae00\ub77c\uc2a4\uc5d0\n\uac78\ub7ec \ub530\ub978\ub2e4.\ub108\ud2b8 \ub9e5\uc744 \uc0b4\uc9dd \ubfcc\ub824\uc900\ub2e4.", "\uc7a5\uc2dd": "\uc601\uad6d\uc655 \uc5d0\ub4dc\uc6cc\ub4dc 7\uc138\uac00 \uacb0\ud63c\uae30\ub150\uc73c\ub85c \uc655\ube44\uc778 \uc54c\ub809\uc0b0\ub4dc\ub77c\uc5d0\uac8c \ubc14\ucce4\ub2e4\ub294 \uce75\ud14c\uc77c\uc774\ub2e4.\n\ucf54\ucf54\uc544\ud5a5\uc774 \ubd80\ub4dc\ub7fd\uace0 \ub9db\uc774 \uc88b\uc544 \uc5ec\uc131\ub4e4\uc5d0\uac8c \ub9e4\uc6b0 \uc778\uae30\ub192\uc740 \uce75\ud14c\uc77c\uc774\ub2e4.", "\uc794": "\uce75\ud14c\uc77c \uae00\ub77c\uc2a4", "\uc720\ub798": "\uc624\ud6c4\uc758 \ubc00\ud68c\ub97c \uc990\uae30\uae30\uc5d0 \uc801\uaca9\uc774\uace0 \ub9c8\uc74c \uc124\ub808\uac8c\ud558\ub294 \ud654\uc0ac\ud55c \uce75\ud14c\uc77c\uc778 \uc544\ub799\uc0b0\ub354\ub294\n\ud5a5\uae0b\ud55c \ube0c\ub79c\ub514\uc5d0 \ud06c\ub818 \ub4dc \uce74\uce74\uc624\uc758 \ucd08\ucf5c\ub9bf \ub9db\uacfc \ud06c\ub9bc\uc758 \ubd80\ub4dc\ub7ec\uc6b4 \uac10\uac01\uc774 \uc77c\ud488\uc778 \uce75\ud14c\uc77c\n\uc54c\ub799\uc0b0\ub354\uc758 \ud0c4\uc0dd\uc124\uc5d0\ub294 \uc601\uad6d\uc655\uc758 \uc655\ube44\uc778 \uc544\ub809\uc0b0\ub4dc\ub9ac\ub77c\uc5d0\uac8c \ubc14\ucce4\ub2e4\ub294 \uc124\uacfc\n\uc655\uc2e4\uc758 \ud63c\ub840\ucd95\uc81c\uc6a9\uc73c\ub85c \uc0ac\uc6a9\ud588\ub2e4\ub294 \uc124\uc774 \uc788\ub2e4.", "\ub9db": "\ucf54\ucf54\uc544 \ud5a5\uc758 \ubd80\ub4dc\ub7ec\uc6b4 \ub9db"}, "img_src": "\ube0c\ub79c\ub514 \ubca0\uc774\uc2a4_files/blank.gif", "name": "\uc54c\ub809\uc0b0\ub354(Alexande)"}, {"descs": {"\uae30\ubc95": "\ube0c\ub79c\ub4dc(Blend)", "\uc7ac\ub8cc": "\ube0c\ub79c\ub514 [1 ml], \ubca0\ub124\ub515\ud2f4 [15 ml]", "\ub9cc\ub4dc\ub294 \ubc29\ubc95": "\uae00\ub77c\uc2a4\uc5d0 \ubca0\ub124\ub515\ud2f4\uc744 \ub530\ub978 \ub2e4\uc74c \ube0c\ub79c\ub514\ub97c \uc870\uc2ec\uc2a4\ub7fd\uac8c \ube44\uc911\uc744 \uc774\uc6a9\ud558\uc5ec \ub744\uc6cc\uc900\ub2e4.", "\uc794": "\ub9ac\ud050\uc5b4 \uae00\ub77c\uc2a4", "\uba54\ubaa8": "\ubca0\ub124\ub515\ud150\uc758 B, \ube0c\ub79c\ub514\uc758 B\uc790\ub97c \ub530\uc11c \uc774\ub984 \uc9c0\uc740 \uc774\ub2c8\uc15c \uce75\ud14c\uc77c\ub85c \ub2ec\ucf64\ud558\uc9c0\ub9cc \ube0c\ub79c\ub514\uc758 \uc9c4\ud55c \ud5a5\uae30\uc640\n \uce75\ud14c\uc77c\uc758 \uc911\ud6c4\ud568\uc744 \ub354\ud574\uc8fc\uace0 \uc788\ub2e4.", "\uc751\uc6a9": "\uc5bc\uc74c\uc744 \ub123\uc740 '\uc628\ub354\ub77d\uc2a4'\ub85c\ub3c4 \uac00\ub2a5\ud558\ub2e4.", "\ub9db": "\ube0c\ub79c\ub514\uc758 \uc9c4\ud55c \ud5a5\uacfc \ub2ec\ucf64\ud55c \ub9db"}, "img_src": "\ube0c\ub79c\ub514 \ubca0\uc774\uc2a4_files/alexande.jpg", "name": "\ube44 \uc564\ub4dc \ube44(B & B)"}, {"descs": {"\uae30\ubc95": "\ud754\ub4e4\uae30(Shake)", "\uc7ac\ub8cc": "\ube0c\ub79c\ub514 [1 ml], \ud06c\ub9bc [90 ml], \uc124\ud0d5\uc2dc\ub7fd [2 tsp], \uacc4\ub780 [1 \uac1c]", "\ub9cc\ub4e4\uae30": "\uc5bc\uc74c\uacfc \uc7ac\ub8cc\ub97c \ubaa8\ub450 \uc250\uc774\ucee4\uc5d0 \ub123\uace0 \ud798\ucc28\uac8c \ud754\ub4e4\uc5b4 \uc794\uc5d0 \ub530\ub978 \ud6c4 \ub11b\uba55\uc744\n\t \uc0b4\uc9dd \ubfcc\ub9b0\ub2e4.\t\ud754\ub4e4 \ub54c\uc5d0\ub294 \uacc4\ub780\uc774 \uc798 \ud480\uc5b4\uc9c0\ub3c4\ub85d \uc798 \ud754\ub4dc\ub294 \uac83\uc774 \uc88b\ub2e4.\n\t \uc8fc\uc758\ud560 \uc810\uc740 \uc2e0\uc120\ud55c \uacc4\ub780\uc744 \uace8\ub77c\uc57c \ud55c\ub2e4.", "\uc794": "8\uc628\uc2a4 \uace0\ube14\ub81b(240ml)", "\uba54\ubaa8": "\uacfc\uac70\uc5d0\ub294 \ub7fc\uc744 \ub354 \ub123\uae30\ub3c4 \ud558\uc600\uc73c\ub098 \ubd80\ub4dc\ub7ec\uc6b4 \uce75\ud14c\uc77c\uc744 \ub9cc\ub4e4\uae30 \uc704\ud558\uc5ec \ub123\uc9c0\n\t\uc54a\ub294\ub2e4. \uc704\uc758 \uc7ac\ub8cc\uc5d0\uc11c \ube0c\ub79c\ub514\ub97c \ub123\uc9c0 \uc54a\uc73c\uba74 \uadf8\ub0e5 '\uc5d0\uadf8\ub099' \ub610\ub294 '\ubc00\ud06c\uc250\n\t\uc774\ud06c'\ub77c \ud55c\ub2e4.", "\uc751\uc6a9": "\uc2e0\uc120\ud558\uc9c0 \uc54a\uc740 \uacc4\ub780\uc73c\ub85c \ub9cc\ub4e4\uc5c8\uc744 \ub54c\uc5d0\ub294 \uce75\ud14c\uc77c\uc758 \ub9db\uacfc \ud5a5\uc744 \ub5a8\uc5b4\ub728\ub9b4\n\t \ubfd0\ub9cc \uc544\ub2c8\ub77c \uacbd\uc6b0\uc5d0 \ub530\ub77c \ub2e4\ub978 \uc7ac\ub8cc\ub3c4 \ud568\uaed8 \ubc84\ub9ac\uace0 \ub2e4\uc2dc \ub9cc\ub4e4\uc5b4\uc57c \ud55c\ub2e4.", "\ub9db": "\uc6b0\uc720\ub9db\uc774 \uac15\ud55c \ub2e8\ub9db, \uc62c\ub370\uc774 \ud0c0\uc785"}, "img_src": "\ube0c\ub79c\ub514 \ubca0\uc774\uc2a4_files/blank.gif", "name": "\ube0c\ub79c\ub514 \uc5d0\uadf8\ub099(Brandy Eggnog)"}, {"descs": {"\uae30\ubc95": "\ud754\ub4e4\uae30(Shake)", "\uc7ac\ub8cc": "\ube0c\ub79c\ub514 [1 ml], \ub808\ubaac\uc8fc\uc2a4 [15 ml], \uc124\ud0d5 [1 tsp]", "\uc7a5\uc2dd": "\ub808\ubaac \uc2ac\ub77c\uc774\uc2a4, \uccb4\ub9ac", "\ub9cc\ub4e4\uae30": "\uc250\uc774\ucee4\uc5d0 \uc7ac\ub8cc\ub97c \uc5bc\uc74c\uacfc \ud568\uaed8 \ud754\ub4e4\uc5b4\uc11c \ub0c9\uac01\ub41c \uc794\uc5d0 \ub530\ub978\ub2e4.", "\uc794": "\uce75\ud14c\uc77c \uae00\ub77c\uc2a4", "\uba54\ubaa8": "\uc54c\ub798\uc2a4\uce74\ub294 \uc11d\uc720,\uad11\ubb3c\uc790\uc6d0\uc774 \ud48d\ubd80\ud55c \ubbf8\uad6d\uc5d0\uc11c \uac00\uc7a5 \ub113\uc740 \uc8fc.\n\uc774 \ub545\uc744 1876\ub144\uc5d0 \ub7ec\uc2dc\uc544\ub85c\ubd80\ud130 \uc0ac\ub4e4\uc778 \uac12\uc740 1\ubca0\uc774\ucee4\ub2f9 \ubd88\uacfc 2\uc13c\ud2b8\uc600\ub2e4.", "\uc751\uc6a9": "\uc18c\ub7c9\uc758 \uc18c\ub2e4\uc218\ub97c \ucca8\uac00\ud558\uae30\ub3c4 \ud55c\ub2e4."}, "img_src": "\ube0c\ub79c\ub514 \ubca0\uc774\uc2a4_files/b&b.jpg", "name": "\ube0c\ub79c\ub514\uc0ac\uc6cc(Brandy Sour)"}, {"descs": {"\uae30\ubc95": "\ud754\ub4e4\uae30(Shake)", "\uc7ac\ub8cc": "\ube0c\ub79c\ub514 [1 ml], \uccb4\ub9ac \ube0c\ub79c\ub514 [15 ml], \ud2b8\ub9ac\ud50c \uc139 [10 ml], \n\uadf8\ub808\ub098\ub518 \uc2dc\ub7fd [15 ml], \ub808\ubaac \uc8fc\uc2a4 [2 tsp]", "\uc7a5\uc2dd": "\uccb4\ub9ac\ub97c \uc794 \uc548\uc5d0 \ub123\uc5b4\ub3c4 \uc88b\uace0 \uc5f7\uc740 \uce75\ud14c\uc77c\uc758 \uc0c9\uc744 \uc990\uae30\ub824\uba74 \uc548\ub123\uc5b4\ub3c4 \ubb34\ubc29\n\t\ud558\ub2e4.", "\ub9cc\ub4e4\uae30": "\uc7ac\ub8cc\ub97c \ubaa8\ub450 \uc250\uc774\ucee4\uc5d0 \uc5bc\uc74c\uacfc \ud568\uaed8 \ub123\uace0 \uc798 \ud754\ub4e0 \ub2e4\uc74c \uc794\uc5d0 \uc5bc\uc74c\uc744 \ub123\uace0\n\t \ub530\ub978\ub2e4.", "\uc794": "6\uc628\uc2a4 \ud544\uc2a4\ub108(180ml)", "\uba54\ubaa8": "\ubc9a\uaf43\uc774 \ud55c\ucc3d\uc774\ub77c\ub294 \ub73b\uc758 \uc774\ub984\uc744 \uac00\uc9c4 \uc740\uc740\ud55c \ubd84\uc704\uae30\uc758 \uce75\ud14c\uc77c\uc774\ub2e4.", "\uc751\uc6a9": "\uae30\ud638\uc5d0 \ub530\ub77c \uc5bc\uc74c\uc5c6\uc774 \uce75\ud14c\uc77c \uc794\uc5d0 \ub530\ub974\uace0 \uccb4\ub9ac 1\uac1c\ub97c \uc7a5\uc2dd\ud558\ub294 \uacbd\uc6b0\ub3c4 \uc788\ub2e4.", "\ub9db": "\ubc97 \uaf43 \ubd84\uc704\uae30\uc758 \ubd80\ub4dc\ub7fd\uace0 \uc0c1\ud07c\ud55c \uc57d\uac04 \ub2e8\ub9db, \uc62c\ub370\uc774 \ud0c0\uc785."}, "img_src": "\ube0c\ub79c\ub514 \ubca0\uc774\uc2a4_files/blank.gif", "name": "\uccb4\ub9ac \ube14\ub7ec\uc12c(Cherry Blossom)"}, {"descs": {"\uae30\ubc95": "\uc9c1\uc811\ub123\uae30(Build)", "\uc7ac\ub8cc": "\ube0c\ub79c\ub514 [1 ml], \uce7c\ub8e8\uc544 [20 ml]", "\ub9cc\ub4e4\uae30": "\uc794\uc5d0 \uc5bc\uc74c\uacfc \uc7ac\ub8cc\ub97c \ub123\uace0 \uac00\ubccd\uac8c \uc813\ub294\ub2e4.", "\uc794": "6\uc628\uc2a4 \ub77d(180ml)", "\uba54\ubaa8": "\uc774\uac83\uc740 \ud22c \ub9ac\ucee4 \ub4dc\ub9c1\ud06c\uc758 \uc77c\uc885\uc73c\ub85c \ub300\ub2e8\ud788 \uc720\uba85\ud558\uc5ec \uc2dd\ud6c4\uc5d0 \ub9e4\uc6b0 \uc88b\ub2e4.", "\uc751\uc6a9": "\ube0c\ub79c\ub514 \ubca0\uc774\uc2a4\ub97c \ubcf4\ub4dc\uce74\ub85c \ubc14\uafb8\uba74 '\ube14\ub799 \ub7ec\uc2dc\uc548(Black Russian)'\uc774 \ub41c\ub2e4.", "\ub9db": "\ucee4\ud53c\ud5a5\uc758 \ub2e8\ub9db, \uc2dd\ud6c4."}, "img_src": "\ube0c\ub79c\ub514 \ubca0\uc774\uc2a4_files/brandyeg.jpg", "name": "\ub354\ud2f0 \uba38\ub354(Dirty Mother)"}, {"descs": {"\uae30\ubc95": "\uc9c1\uc811\ub123\uae30(Build)", "\uc7ac\ub8cc": "\ube0c\ub79c\ub514 [1 ml], \ub808\ubaac [1 \uac1c]", "\uc7a5\uc2dd": "\ub808\ubaac\uaecd\uc9c8, \uc2a4\ud2b8\ub85c", "\ub9cc\ub4e4\uae30": "\uae00\ub77c\uc2a4\uc5d0 \ub808\ubaac\uc758 \uaecd\uc9c8\uc744 \ub098\uc120\ud615\uc73c\ub85c \ub3cc\ub824 \ubc97\uaca8 \ub05d\uc744 \uae00\ub77c\uc2a4\uc5d0 \uac78\uccd0 \ub193\ub294\ub2e4.\n\t \uc5bc\uc74c\uc744 \ub123\uace0 \ube0c\ub79c\ub514\ub97c \ub123\ub294\ub2e4.", "\uc794": "\ucf5c\ub9b0\uc988 \uae00\ub77c\uc2a4", "\uba54\ubaa8": "\uc794\uc5d0 \uac78\uccd0\uc788\ub294 \ub808\ubaac\ud615\ud0dc\uac00 \ub9d0\ubaa9\ucc98\ub7fc \uc0dd\uacbc\ub2e4 \ud574\uc11c \ubd99\uc5ec\uc9c4 \uc774\ub984\uc778\ub370 \ubbf8\uad6d \uc81c\n\t26\ub300 \ub300\ud1b5\ub839 \ub8e8\uc988\ubca8\ud2b8\uac00 \uc2b9\ub9c8\ud560 \ub54c\uc5d0 \ub9d0\uc758 \ubaa9\uc744 \uc4f0\ub2e4\ub4ec\uc73c\uba70 \uc990\uaca8 \ub9c8\uc168\ub2e4\uace0\n\t\uc804\ud574\uc9c4\ub2e4.", "\uc751\uc6a9": "\ud638\uc2dc\uc988 \ub125\uc758 \uae30\uc8fc\ub294 \ube0c\ub79c\ub514\ubfd0\ub9cc \uc544\ub2c8\ub77c \uc704\uc2a4\ud0a4, \ub7fc, \uc9c4 \ub4f1 \uc5ec\ub7ec\uac00\uc9c0 \uae30\uc8fc\ub97c\n\t\uc0ac\uc6a9\ud560 \uc218 \uc788\ub2e4. \n\t8\uc628\uc2a4 \ud558\uc774\ubcfc \uc794\uc5d0 \ube0c\ub79c\ub514\uc640 \uc9c4\uc800\uc5d8\uc744 \uc11e\uace0 \ub808\ubaac\uaecd\uc9c8\uc744 \ub123\uc9c0 \uc54a\uc73c\uba74 '\ube0c\ub79c\ub514\n\t\uc9c4\uc800\uc5d8'\uc774 \ub41c\ub2e4.", "\ub9db": "\ubc15\ud558\ud5a5\uacfc \ube0c\ub79c\ub514\uc758 \uc790\uadf9\uc801\uc778 \ub9db"}, "img_src": "\ube0c\ub79c\ub514 \ubca0\uc774\uc2a4_files/blank.gif", "name": "\ud638\uc2dc\uc988 \ub125(Horse's Neck)"}, {"descs": {"\uae30\ubc95": "\uc9c1\uc811\ub123\uae30(Build)", "\uc7ac\ub8cc": "\ube0c\ub79c\ub514 [1 ml], \uac00\ub8e8\uc124\ud0d5 [1 tsp], \ub465\uadfc \uc2ac\ub77c\uc774\uc2a4\ub808\ubaac [1 \uac1c]", "\uc7a5\uc2dd": "\ub465\uadfc \uc2ac\ub77c\uc774\uc2a4 \ub808\ubaac, \uac00\ub8e8\uc124\ud0d5.", "\ub9cc\ub4e4\uae30": "\uba3c\uc800 \uc785\uc774 \uc881\uc740 \ub9ac\ud050\ub974 \uc794\uc5d0 \uc124\ud0d5\uac00\ub8e8\ub97c \ub123\uace0 \uc900\ube44\ub41c \uc2ac\ub77c\uc774\uc2a4 \ub808\ubaac \uc704\uc5d0\n\t \uc5ce\uc5b4\uc11c \uc3df\uc544 \ub193\uc740 \ub2e4\uc74c \uc794\uc5d0 \ube0c\ub79c\ub514\ub97c \ubd93\uace0 \uc124\ud0d5\uc774 \uc5b9\ud600 \uc788\ub294 \ub808\ubaac\uc744 \uc794 \uc704\n\t \uc5d0 \uc62c\ub824 \ub193\ub294\ub2e4.", "\uc794": "\ub9ac\ud050\ub974", "\uba54\ubaa8": "\ub9c8\uc2dc\ub294 \ubc29\ubc95\uc740 \ub450\uc190 \ub05d\uc73c\ub85c \ub808\ubaac\uc744 \uac10\uc2f8\uc950\uc5b4 \uc124\ud0d5\uacfc \ud568\uaed8 \uc785\uc548\uc5d0 \ub123\uc740 \ub2e4\uc74c\n\t\ube0c\ub79c\ub514\ub97c \ub9c8\uc154 \uc785\uc548\uc5d0\uc11c \uce75\ud14c\uc77c\uc744 \ub9cc\ub4e4\uba74\uc11c \uc74c\ubbf8\ud55c\ub2e4.", "\uc751\uc6a9": "\uae30\ud638\uc5d0 \ub530\ub77c\uc11c \uc124\ud0d5 \uc704\uc5d0 \uc778\uc2a4\ud0c4\ud2b8 \ucee4\ud53c\ub97c \uc5b9\uc5b4 \ub193\uae30\ub3c4 \ud55c\ub2e4.", "\ub9db": "\uac01 \uc7ac\ub8cc\uc758 \uac15\ud55c \ub9db, \uc2dd\ud6c4."}, "img_src": "\ube0c\ub79c\ub514 \ubca0\uc774\uc2a4_files/brandyso.jpg", "name": "\ub2c8\ucf5c\ub77c\uc2dc\uce74(Nikolashika)"}, {"descs": {"\uae30\ubc95": "\ud754\ub4e4\uae30(Shake)", "\uc7ac\ub8cc": "\ube0c\ub79c\ub514 [1 ml], \ud654\uc774\ud2b8 \ud050\ub77c\uc18c [7.5 ml], \ub808\ubaac\uc8fc\uc2a4 [7.5 ml]", "\ub9cc\ub4e4\uae30": "\uc250\uc774\ucee4\uc5d0 \uc7ac\ub8cc\ub97c \uc5bc\uc74c\uacfc \ud568\uaed8 \ub123\uace0 7-8\ud68c \ud63c\ud569\ud558\uc5ec \ub0c9\uac01\ub41c \uc794\uc5d0 \ub530\ub978\ub2e4.", "\uc794": "\uce75\ud14c\uc77c \uae00\ub77c\uc2a4", "\uba54\ubaa8": "\ub9c8\ud2f0\ub2c8, \ub9e8\ud558\ud0c4\uacfc \ud568\uaed8 \ub9ce\uc740 \uc774\ub4e4\uc774 \uc560\uc74c\ud558\uace0 \uc788\ub2e4.\n\t\uc77c\uc124\uc5d0 \uc758\ud558\uba74 \uc138\uacc4 \uc81c1\ucc28\ub300\uc804 \uc911 \ub3c5\uc77c\uad70\uc758 \ub9f9\uacf5\uc73c\ub85c \ucad3\uae30\ub358 \ud504\ub791\uc2a4 \uc7a5\uad50\uac00\n\t\uc0ac\uc774\ub4dc\uce74\ub85c \ud1f4\uac01 \uc911 \ubaa9\uc744 \ucd95\uc774\uae30 \uc704\ud574 \uc5bc\ub9c8 \ub0a8\uc9c0 \uc54a\uc740 \ube0c\ub79c\ub514\uc5d0 \ud050\ub77c\uc18c\uc640\n\t\ub808\ubaac\uc8fc\uc2a4\ub97c \uc11e\uc5b4 \ub9c8\uc2e0 \uac83\uc774 \uc720\ub798\uac00 \ub418\uc5c8\ub2e4 \ud55c\ub2e4.", "\uc751\uc6a9": "\ube0c\ub79c\ub514 \ub300\uc2e0 \uc9c4\uc744 \uc0ac\uc6a9\ud558\uba74 '\ud654\uc774\ud2b8 \ub808\uc774\ub514(White Lady)', \ub77c\uc774\ud2b8 \ub7fc\uc744 \uc0ac\n\t\uc6a9\ud558\uba74 '\uc5d1\uc2a4 \uc640\uc774 \uc9c0(X.Y.Z.)'\ub77c\ub294 \uce75\ud14c\uc77c\uc774 \ub41c\ub2e4.", "\ub9db": "\uc911\uac04 \ub2e8\ub9db, \uc62c\ub370\uc774 \ud0c0\uc785.", "\uc720\ub798": "\uc720\ub7fd \uad70\uc778\ub4e4 \uc0ac\uc774\uc5d0\uc11c \uc0ac\ub791\uc744 \ubc1b\uc544 \uc804\uc138\uacc4\ub85c \ubed7\uc5b4\ub098\uac04 \uace0\uc804\uc801 \uce75\ud14c\uc77c \uc0ac\uc774\n\t\ub4dc\uce74\ub294 \uc250\uc774\ud06c\ud558\uc5ec \ub9cc\ub4dc\ub294 \uce75\ud14c\uc77c\uc758 \uae30\ubcf8\uc774\ub77c \ubd88\ub9ac\ub294 \ub300\ud45c\uc801\uc778 \uce75\ud14c\uc77c\uc774\ub2e4."}, "img_src": "\ube0c\ub79c\ub514 \ubca0\uc774\uc2a4_files/blank.gif", "name": "\uc0ac\uc774\ub4dc \uce74(Side Car)"}, {"descs": {"\uae30\ubc95": "\ud754\ub4e4\uae30(Shake)", "\uc7ac\ub8cc": "\ube0c\ub79c\ub514 [1 ml], \ud06c\ub9bc\ub4dc \ubbfc\ud2b8(\ud654\uc774\ud2b8) [15 ml]", "\ub9cc\ub4e4\uae30": "\uc250\uc774\ucee4\uc5d0 \uc7ac\ub8cc\ub97c \uc5bc\uc74c\uacfc \ud568\uaed8 \uc798 \uc11e\uc5b4 \ub0c9\uac01\ub41c \uc794\uc5d0 \uac78\ub7ec \ub530\ub978\ub2e4.", "\uc794": "\uce75\ud14c\uc77c \uae00\ub77c\uc2a4", "\uba54\ubaa8": "\uc2a4\ud305\uac70\ub780 \"\ucc0c\ub974\ub294 \ubc14\ub298\uc774\ub2e4\"\ub77c\ub294 \ub73b\uc73c\ub85c \uc774 \uce75\ud14c\uc77c\uc774 \uc785\uc548\uc5d0\uc11c \uc790\uadf9\uc801\uc784\uc744\n\t\uc554\uc2dc\ud558\uace0 \uc788\ub2e4.", "\uc751\uc6a9": "\uc250\uc774\ud06c\ud558\uc9c0 \uc54a\uace0 \ub77d\uadf8\ub77c\uc2a4\uc5d0 \uc5bc\uc74c\uc744 2-3\uac1c \ub123\uace0 \uc9c1\uc811 \ub123\uc73c\uba74 '\uc2a4\ud305\uac70 \uc628\ub354\n\t\ub77d\uc2a4'\ub77c\uace0 \ubd80\ub974\uba70 \ud604\ub300 \uce75\ud14c\uc77c\uc740 \uac15\ud55c \ub9db\uc774 \ub098\ub294 \uc2a4\ud2b8\ub808\uc774\ud2b8\uc5c5(Straight \n\tup)\ubcf4\ub2e4 \uc5bc\uc74c\uc744 \ub123\uc5b4 \uc628\ub354\ub77d\uc2a4\ub97c \uc120\ud638\ud558\ub294 \uacbd\ud5a5\uc774\ub2e4.", "\ub9db": "\ubc15\ud558\ud5a5\uacfc \ube0c\ub79c\ub514\uc758 \uc790\uadf9\uc801\uc778 \ub9db.", "\uc720\ub798": "\uc2dd\ud6c4\uc8fc\ub85c \uc778\uae30\uac00 \ub192\uc740 \uc0b0\ub73b\ud55c \ubbfc\ud2b8\ub9db\uc758 \uc2a4\ud305\uac70\ub294 \uc18c\ud654\ub97c \ub3d5\uace0 \uc704\uc758 \ubd80\ub2f4\uac10\n\t\uc744 \ub35c\uc5b4\uc8fc\ub294 \ud6a8\uacfc\ub97c \uac00\uc9c4 \uc2dd\ud6c4\uc8fc."}, "img_src": "\ube0c\ub79c\ub514 \ubca0\uc774\uc2a4_files/cherrybl.jpg", "name": "\uc2a4\ud305\uac70(Stinger)"}, {"descs": {"\uae30\ubc95": "\ud754\ub4e4\uae30(Shake)", "\uc7ac\ub8cc": "\ube0c\ub79c\ub514 [1 ml], \ub7fc [15 ml], \ud2b8\ub9ac\ud50c \uc139 [15 ml], \ub808\ubaac\uc8fc\uc2a4 [15 ml]", "\ub9cc\ub4e4\uae30": "\uc250\uc774\ucee4\uc5d0 \uc5bc\uc74c\uacfc \uc7ac\ub8cc\ub97c \ub123\uace0 \ud754\ub4e4\uc5b4 \uc794\uc5d0 \uac78\ub7ec \ub530\ub978\ub2e4.", "\uc794": "3\uc628\uc2a4 \uce75\ud14c\uc77c(90ml)", "\uba54\ubaa8": "\uc7a0\uc790\ub9ac\uc5d0 \ub4e4\uae30 \uc804\uc5d0 \ub9c8\uc2dc\ub294 \uce68\uc804\uc8fc(Night Cap)\ub85c \uc798 \uc54c\ub824\uc838 \uc788\ub2e4.", "\ub9db": "\uc57d\uac04 \ub2e8\ub9db\uc758 \uac15\ud55c \ub9db, \uc2dd\ud6c4."}, "img_src": "\ube0c\ub79c\ub514 \ubca0\uc774\uc2a4_files/blank.gif", "name": "\ube44\ud2b8\uc708 \ub354 \uc26c\uce20(Between The Sheets)"}, {"descs": {"\uae30\ubc95": "\ubb3b\ud788\uae30(Rimmed With Sugar)\uc640 \ud754\ub4e4\uae30(Shake)", "\uc7ac\ub8cc": "\ube0c\ub79c\ub514 [1 ml], \ud2b8\ub9ac\ud50c \uc139 [15 ml], \ub9c8\ub77c\uc2a4\ud0a4\ub178 [1 tsp], \ube44\ud130\uc988 [1 Dash]", "\uc7a5\uc2dd": "\ub808\ubaac\uc744 \ub098\uc120\ud615\uc73c\ub85c \ubc97\uae34 \uaecd\uc9c8, \uc18c\uae08 \ubb3b\ud798, \uc2a4\ud2b8\ub85c", "\ub9cc\ub4e4\uae30": "\uba3c\uc800 \uc18c\uae08\uc744 \ud3c9\ud3c9\ud55c \uadf8\ub987\uc5d0 \ud3b4 \ub193\uc740 \ub2e4\uc74c \uc370\uc740 \ub808\ubaac\uc5d0 \uc794\uc758 \uac00\uc7a5\uc790\ub9ac\ub97c\n\t  \uc801\uc154\uc11c \uac70\uafb8\ub85c \ud558\uc5ec \uc18c\uae08\uc744 \ubb3b\ud600 \ub193\ub294\ub2e4. \ub2e4\uc74c\uc740 \ub808\ubaac\uc744 \ub098\uc120\ud615\uc73c\ub85c \uaecd\uc9c8\n\t  \uc744 \ubc97\uaca8 \uc794\uc5d0 \uac78\uccd0 \ub193\ub294\ub2e4. \uc5bc\uc74c\uacfc \uc7ac\ub8cc\ub97c \uc250\uc774\ucee4\uc5d0 \ub123\uace0 \uc798 \ud754\ub4e4\uc5b4 \uc794\uc5d0\n\t  \ub530\ub974\uace0 \uc5bc\uc74c\uc744 \ub123\ub294\ub2e4.", "\uc794": "6\uc628\uc2a4 \ud544\uc2a4\ub108(180ml)", "\uba54\ubaa8": "\ud06c\ub7ec\uc2a4\ud2b8\ub294 \uaecd\uc9c8 \ub610\ub294 \ub531\ub531\ud55c \ud3c9\uba74\uc744 \ub73b\ud558\ub294\ub370 \ub808\ubaac\uc758 \uaecd\uc9c8\uc744 \ub123\uc740\ub370\uc11c \uc774\n\t\ub7ec\ud55c \uc774\ub984\uc774 \ubd99\uc5ec\uc9c0\uac8c \ub418\uc5c8\ub2e4.", "\ub9db": "\uac00\ub8e8\uc124\ud0d5 \ub54c\ubb38\uc5d0 \uccab\ub9db\uc740 \ub2ec\uace0 \ub4b7\ub9db\uc740 \ud5a5\uae30\ub86d\ub2e4, \uc62c\ub370\uc774 \ud0c0\uc785.", "\uc720\ub798": "\uc0c1\ucf8c\ud55c \ub808\ubaac\uc758 \ud48d\ubbf8\uac00 \uc11c\ub298\ud55c \ub290\ub08c\uc744 \uac00\uc838\ub2e4 \uc8fc\ub294 \ub4dc\ub77c\uc774 \ucde8\ud5a5\uc758 \uce75\ud14c\uc77c \ube0c\n\t\ub79c\ub514 \ud06c\ub7ec\uc2a4\ud0c0\ub294 \ub808\ubaac \ud5a5\uae30\uac00 \uadf8\uc73d\ud55c \uce75\ud14c\uc77c. \ube0c\ub79c\ub514\uc758 \uac08\uc0c9\uacfc \ub808\ubaac\uc758 \ud669\uc0c9\n\t\uc774 \uc5b4\uc6b8\ub824 \uc544\ub984\ub2e4\uc6b4 \uc0c9\uae54\uc744 \ube5b\uc5b4\ub0b8\ub2e4.\uc4f4 \ub9db\uc758 \uce75\ud14c\uc77c\uc774 \uc720\ud589\ud558\ub358 \uc2dc\ub300\uc5d0\ub294 \n\t\ud06c\uac8c \uc778\uae30\ub97c \uc5bb\uc5c8\uc73c\ub098 \uac00\ubccd\uace0 \uc21c\ud55c \uac10\uac01\uc758 \uce75\ud14c\uc77c\uc774 \uc720\ud589\ud558\ub294 \uc624\ub298\ub0a0\uc5d0\ub294 \n\t\ucc3e\ub294 \uc0ac\ub78c\uc774 \uc57d\uac04 \uc801\uc5b4\uc84c\ub2e4. \ub2e8\uac83\uc744 \uc88b\uc544\ud558\ub294 \uc0ac\ub78c\uc740 \uc288\uac70 \uc2dc\ub7fd\uc744 \ud0c0\uba74 \uc88b\ub2e4."}, "img_src": "\ube0c\ub79c\ub514 \ubca0\uc774\uc2a4_files/dirtymot.jpg", "name": "\ube0c\ub79c\ub514 \ud06c\ub7ec\uc2a4\ud0c0(Brandy Crusta)"}]